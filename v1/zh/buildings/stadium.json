    "game_info": "\u9ad4\u80b2\u5834\u7121\u6cd5\u63d0\u4f9b\u8d85\u904e\u57ce\u5e02\u4e2d\u5e02\u6c11\u4eba\u6578\u7684\u5e78\u798f\u5ea6\u3002\u57ce\u5e02\u5fc5\u9808\u64c1\u6709\u4e00\u5bb6\u52d5\u7269\u5712\u3002",
    "strategy": "\u9ad4\u80b2\u5834\u662f\u73fe\u4ee3\u7684\u5efa\u7bc9\uff0c\u5b83\u80fd\u63d0\u9ad8\u6574\u500b\u5e1d\u570b\u7684\u5e78\u798f\u5ea6\uff0c\u4f46\u63d0\u9ad8\u7684\u6578\u503c\u4e0d\u6703\u8d85\u904e\u8a72\u57ce\u5e02\u7684\u5e02\u6c11\u4eba\u6578\u3002\u57ce\u5e02\u5728\u5efa\u9020\u9ad4\u80b2\u5834\u524d\u5fc5\u9808\u5148\u64c1\u6709\u4e00\u5bb6\u52d5\u7269\u5712\u3002",
    "historical_info": "\u9ad4\u80b2\u5834\u5c31\u662f\u73fe\u4ee3\u7248\u7684\u7af6\u6280\u5834\uff0c\u4e0d\u904e\u9ad4\u80b2\u5834\u5c0f\u5403\u61c9\u8a72\u6bd4\u7af6\u6280\u5834\u8981\u4f86\u5f97\u8c50\u5bcc\u7f8e\u5473\u3002\u73fe\u4ee3\u9ad4\u80b2\u5834\u53ef\u4ee5\u5bb9\u7d0d\u8d85\u904e\u5341\u842c\u540d\u760b\u72c2\u5c16\u53eb\u7684\u904b\u52d5\u7c89\u7d72\uff0c\u9019\u4e9b\u4eba\u8702\u6e67\u800c\u81f3\uff0c\u70ba\u7684\u5c31\u662f\u770b\u5834\u597d\u7403\u8cfd\u3001\u53c3\u52a0\u6f14\u5531\u6703\u6216\u662f\u653f\u6cbb\u96c6\u6703\u3002\u73fe\u4ee3\u7684\u904b\u52d5\u8ff7\u5011\u8207\u53e4\u5178\u6587\u5316\u6642\u671f\u7684\u53e4\u4eba\u5011\u76f8\u8f03\u4e4b\u4e0b\uff0c\u4e26\u6c92\u6709\u8b8a\u5f97\u6bd4\u8f03\u6587\u660e\u6709\u79e9\u5e8f\uff0c\u9019\u4e5f\u662f\u70ba\u4ec0\u9ebc\u4e00\u76f4\u5230\u4eca\u5929\uff0c\u6211\u5011\u7684\u9ad4\u80b2\u5834\u88e1\u4ecd\u7136\u6709\u570d\u7246\u3001\u8b77\u6b04\uff0c\u800c\u4e14\u9084\u4f48\u7f72\u4e86\u5927\u6279\u7684\u4fdd\u5168\u4eba\u54e1\uff0c\u96a8\u6642\u6e96\u5099\u5354\u52a9\u8655\u7406\u6492\u91ce\u9b27\u4e8b\u7684\u89c0\u773e\u3002",
