    "game_info": "\u66ff\u4ee3\u99ac\u5ec4\u7684\u6ce2\u862d\u7279\u8272\u5efa\u7b51\u3002\u751f\u7522\u9a0e\u4e58\u55ae\u4f4d\u6642+15%\u751f\u7522\u529b\u53ca+15\u7d93\u9a57\u503c\u3002\u96b8\u5c6c\u65bc\u6b64\u57ce\u5e02\u7684\u6bcf\u8655\u7267\u5834+1\u751f\u7522\u529b\u548c+1\u91d1\u5e63\u3002",
    "strategy": "\u516c\u7235\u99ac\u5ec4\u9664\u4e86\u64c1\u6709\u4e00\u822c\u99ac\u5ec4\u7684\u529f\u80fd\u5916\uff0c\u9084\u80fd\u63d0\u9ad8\u7267\u5834\u55ae\u5143\u683c\u7684\u91d1\u5e63\u4ee5\u53ca\u70ba\u9a0e\u4e58\u55ae\u4f4d\u63d0\u4f9b+15\u7d93\u9a57\u503c\u3002\u50c5\u9650\u6ce2\u862d\u5efa\u9020\u3002",
    "historical_info": "\u5361\u9f4a\u7c73\u65e5\u4e09\u4e16\u65bc\u897f\u51431333\u5e74\u767b\u4e0a\u5bf6\u5ea7\u5f8c\uff0c\u65cb\u5373\u88ab\u8feb\u5b89\u64ab\u7126\u8e81\u4e0d\u5b89\u7684\u6ce2\u862d\u8cb4\u65cf\uff0c\u800c\u5c0d\u5176\u5927\u5e45\u8b93\u6b65\uff0c\u5305\u62ec\u5141\u8a31\u4ed6\u5011\u64c1\u6709\u76f8\u7576\u65bc\u7687\u5bb6\u7684\u99ac\u5ec4\u6578\u91cf\u3002\u76f4\u5230\u62ff\u7834\u5d19\u6230\u722d\u7d50\u675f\u524d\uff0c\u9019\u4e9b\u516c\u7235\u99ac\u5ec4\u4e00\u76f4\u662f\u8072\u671b\u548c\u6b0a\u529b\u7684\u4f86\u6e90\u3002\u4e0d\u50c5\u53ef\u4f9b\u99ac\u592b\u5c45\u4f4f\uff0c\u9084\u80fd\u5708\u990a50\u5339\u5230\u6578\u767e\u5339\u7684\u99ac\u96bb\uff0c\u4e26\u5305\u62ec\u653e\u7f6e\u5e73\u982d\u91d8\u3001\u99ac\u978d\u8207\u99ac\u98fe\u7684\u5132\u85cf\u5ba4\u3002\u6709\u4e9b\u50cf\u65af\u585e\u65b0\u5730\u5340\u7684\u516c\u7235\u99ac\u5ec4\uff0c\u6ce8\u91cd\u5be6\u7528\u6027\u8207\u529f\u80fd\u6027\uff0c\u6709\u4e9b\u5247\u8ca1\u5927\u6c23\u7c97\uff0c\u4f8b\u5982\u514b\u62c9\u65af\u79d1\u5730\u5340\u7684\u99ac\u5ec4\u5c08\u6ce8\u65bc\u4fee\u5efa\u77f3\u67f1\u548c\u96d5\u50cf\u3002\u7121\u8ad6\u5927\u5c0f\u548c\u7d0b\u98fe\uff0c\u516c\u7235\u7684\u99ac\u5ec4\u662f\u6ce2\u862d\u8457\u540d\u9a0e\u5175\u7de8\u968a\u7684\u5750\u9a0e\u4f86\u6e90\u3002",
