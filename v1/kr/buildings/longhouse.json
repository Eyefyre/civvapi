    "game_info": "\uc232 \ud0c0\uc77c\uc758 \uc0dd\uc0b0\ub825\uc774 1\ub9cc\ud07c \uc99d\uac00\ud569\ub2c8\ub2e4.",
    "strategy": "\ub871\ud558\uc6b0\uc2a4\ub294 \uc791\uc5c5\uc7a5\uc744 \ub300\uccb4\ud558\ub294 \uc774\ub85c\ucffc\uc774\uc871\uc758 \uace0\uc720 \uac74\ubb3c\uc785\ub2c8\ub2e4. \uc774 \uac74\ubb3c\uc740 \ub3c4\uc2dc \ubc18\uacbd \uc548\uc5d0 \uc788\ub294 \uc232 \ud0c0\uc77c\uc758 \uc0dd\uc0b0\ub825\uc744 \ub192\uc5ec\uc90d\ub2c8\ub2e4.",
    "historical_info": "\uae4c\ub9c8\ub4dd\ud788 \uc624\ub798\uc804\ubd80\ud130 \uc774\ub85c\ucffc\uc774\uc871\uc740 \ub290\ub985\ub098\ubb34 \uaecd\uc9c8\uc744 \ub450\ub978 \ubb18\ubaa9\uc73c\ub85c \uc9c0\uc740 \uacf5\ub3d9 \uc8fc\ud0dd\uc5d0 \ubaa8\uc5ec \uc0b4\uc558\ub2e4. '\ub871\ud558\uc6b0\uc2a4'\uc5d0 \ubaa8\uc5ec \uc0b4\ub358 \uc774\ub85c\ucffc\uc774\uc871\uc740 \uc790\uc2e0\ub4e4\uc744 '\ud638\ub370\ub178\uc0ac\uc6b0\ub2c8'\ub77c \ubd80\ub974\ub294\ub370, '\ub871\ud558\uc6b0\uc2a4 \uc0ac\ub78c\ub4e4'\uc774\ub77c\ub294 \ub73b\uc774\ub2e4. \ub871\ud558\uc6b0\uc2a4\ub294 \uae38\uc774\uac00 12m\uc5d0\uc11c 124m\uae4c\uc9c0 \ub2e4\uc591\ud558\uba70 \ud3ed\uc740 \uc57d 8m\uc774\ub2e4. \ub871\ud558\uc6b0\uc2a4 \ub0b4\ubd80\ub294 \uce78\ub9c9\uc774\ub85c \ubd84\ub9ac\ub418\uba70 \uac01 \uce78\uc740 \uac1c\ubc29\ub41c \uc911\uc559 \ud1b5\ub85c\ub85c \uc774\uc5b4\uc9c4\ub2e4. \uc911\uc559 \ud1b5\ub85c\ub97c \ub530\ub77c \ub4dc\ubb38\ub4dc\ubb38 \uc9c0\ubd95\uc774 \ub6ab\ub9b0 \uacf3\uc774 \uc788\uc5b4 \uba87 \uc9d1\ub9c8\ub2e4 \uc694\ub9ac\uc6a9 \ubd88\uc744 \uacf5\uc720\ud55c\ub2e4.\ub871\ud558\uc6b0\uc2a4\ub294 \uac1c\ubcc4 \uac00\uc625\uc5d0 \ube44\ud574 \uac74\ucd95\uacfc \ub09c\ubc29\uc774 \uc6a9\uc774\ud558\uc5ec \ub2e4\ub978 \ud615\ud0dc\uc758 \uc8fc\uac70\ubcf4\ub2e4 \ucc9c\uc5f0\uc790\uc6d0\uc774 \ub35c \ub4e4\uc5b4\uac04\ub2e4. \ub871\ud558\uc6b0\uc2a4\ub294 \uc790\uc5f0\uacfc \uc870\ud654\ub97c \uc774\ub8e8\uba70 \ubc88\uc601\ud558\ub824\ub294 \uc774\ub85c\ucffc\uc774\uc871 \ubb38\ud654\uc758 \ud45c\uc0c1\uc774\ub2e4.",
