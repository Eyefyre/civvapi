    "dawn_of_man": "\u7956\u9b6f\u738b\u570b\u6c99\u5361\uff0c\u5049\u5927\u7684\u6230\u58eb\uff0c\u60a8\u7684\u4eba\u6c11\u5728\u6b64\u4f9b\u60a8\u5dee\u9063\u3002\u60a8\u4ee5\u6d41\u4ea1\u8005\u7684\u8eab\u5206\u5d1b\u8d77\uff0c\u6210\u70ba\u7956\u9b6f\u4eba\u771f\u6b63\u7684\u9818\u5c0e\u8005\uff0c\u8fce\u4f86\u6574\u9ad4\u7956\u9b6f\u793e\u6703\u7684\u6539\u8b8a\u3002\u60a8\u5be6\u65bd\u65b0\u7684\u6cd5\u5f8b\uff0c\u4e26\u9032\u884c\u6587\u5316\u6539\u9769\uff0c\u5f37\u5316\u4e86\u60a8\u7684\u738b\u570b\u7684\u5730\u4f4d\u3002\u7136\u800c\uff0c\u60a8\u5728\u6230\u722d\u85dd\u8853\u4e2d\u8868\u73fe\u7684\u5805\u97cc\u624d\u662f\u771f\u6b63\u70ba\u60a8\u5efa\u7acb\u8072\u8b7d\u7684\u539f\u56e0\uff0c\u56e0\u60a8\u5728\u6230\u5834\u4e0a\u7684\u6230\u8853\u548c\u6230\u7565\u6c38\u4e45\u6539\u8b8a\u4e86\u5357\u975e\u5e73\u539f\u4e0a\u6230\u9b25\u7684\u9762\u8c8c\u3002\u6700\u5f37\u5927\u7684\u570b\u738b\uff0c\u60a8\u7684\u4eba\u6c11\u518d\u6b21\u6e34\u671b\u60a8\u7684\u9818\u5c0e\u5e36\u4f86\u7684\u5718\u7d50\u8207\u529b\u91cf\u3002\u60a8\u53ef\u4ee5\u8b93\u7956\u9b6f\u738b\u570b\u518d\u73fe\u8f1d\u714c\u55ce\uff1f\u60a8\u60f3\u5efa\u7acb\u4e00\u500b\u6b77\u4e45\u5f4c\u65b0\u7684\u6587\u660e\u55ce\uff1f",
