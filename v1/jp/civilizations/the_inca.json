        "titles": [
            "\u30b5\u30d1\u30fb\u30a4\u30f3\u30ab(\u7687\u5e1d)"
        ],
        "historical_info": [
            {
                "heading": "\u6b74\u53f2",
                "text": "\u30af\u30b9\u30b3\u56fd\u306e\u7b2c9\u4ee3\u6307\u5c0e\u8005\u3067\u3042\u308b\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u3001\u5c0f\u3055\u306a\u738b\u56fd\u3092\u5de8\u5927\u306a\u5e1d\u56fd\u30bf\u30ef\u30f3\u30c6\u30a3\u30f3\u30b9\u30a6\u30e6\u3078\u3068\u6210\u9577\u3055\u305b\u305f\u3002\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u6642\u4ee3\u306e\u30a4\u30f3\u30ab\u5e1d\u56fd\u306f\u3001\u73fe\u4ee3\u306e\u30da\u30eb\u30fc\u306e\u5927\u90e8\u5206\u3001\u30dc\u30ea\u30d3\u30a2\u3001\u5317\u90e8\u30a2\u30eb\u30bc\u30f3\u30c1\u30f3\u3092\u542b\u3080\u3001\u30c1\u30ea\u304b\u3089\u30a8\u30af\u30a2\u30c9\u30eb\u306b\u8de8\u308b\u5e1d\u56fd\u3067\u3001\u5f8c\u306b\u3088\u308a\u5927\u304d\u306a\u9818\u571f\u3092\u6301\u3064\u3053\u3068\u306b\u306a\u308b\u5de8\u5927\u306a\u30a4\u30f3\u30ab\u306e\u57fa\u790e\u3068\u306a\u3063\u305f\u3002"
            },
            {
                "heading": "\u738b\u3078\u306e\u5373\u4f4d",
                "text": "\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u30a4\u30f3\u30ab\u30fb\u30a6\u30a3\u30e9\u30b3\u30c1\u30e3\u306e\u606f\u5b50\u3067\u3001\u5144\u30a6\u30eb\u30b3\u306b\u6b21\u3050\u7687\u4f4d\u7b2c\u7d99\u627f\u6a29\u3092\u6301\u3064\u6b21\u7537\u3068\u3057\u3066\u751f\u307e\u308c\u305f\u3002\u3053\u306e\u6642\u306e\u30af\u30b9\u30b3\u738b\u56fd\u306f\u3069\u3061\u3089\u304b\u3068\u3044\u3048\u3070\u5c0f\u56fd\u3067\u3001\u5e38\u306b\u8fd1\u96a3\u306e\u30c1\u30e3\u30f3\u30ab\u65cf\u306e\u8105\u5a01\u306b\u3055\u3089\u3055\u308c\u3066\u3044\u305f\u3002\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306e\u5e7c\u5e74\u6642\u4ee3\u306b\u3064\u3044\u3066\u306f\u3042\u307e\u308a\u77e5\u3089\u308c\u3066\u304a\u3089\u305a\u3001\u30c1\u30e3\u30f3\u30ab\u65cf\u306e\u4fb5\u653b\u6642\u306b\u7236\u306b\u611f\u9298\u3092\u4e0e\u3048\u305f\u3053\u3068\u3067\u3001\u521d\u3081\u3066\u6b74\u53f2\u306b\u767b\u5834\u3059\u308b\u3053\u3068\u306b\u306a\u308b\u3002\u3053\u306e\u6642\u3001\u7236\u3068\u5144\u306f\u6226\u5834\u304b\u3089\u9000\u5374\u3057\u305f\u304c\u3001\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u6b8b\u5b58\u5175\u305f\u3061\u3092\u9f13\u821e\u3057\u3001\u305d\u306e\u65e5\u306e\u3046\u3061\u306b\u52dd\u5229\u3092\u3042\u3052\u305f\u3002\u307e\u305f\u3053\u306e\u6642\u306b\u3001\u3042\u307e\u308a\u306b\u3082\u5fb9\u5e95\u7684\u306b\u30c1\u30e3\u30f3\u30ab\u65cf\u3092\u53e9\u304d\u6f70\u3057\u305f\u305f\u3081\u3001\u5927\u5730\u307e\u3067\u3082\u304c\u305b\u308a\u4e0a\u304c\u308a\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306e\u5473\u65b9\u3092\u3057\u305f\u3001\u3068\u306e\u4f1d\u8aac\u3059\u3089\u751f\u307e\u308c\u305f\u3002\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\uff08\u5927\u5730\u3092\u9707\u308f\u3059\u8005\u3001\u306e\u610f\uff09\u306f\u3053\u306e\u5f8c\u3001\u7687\u592a\u5b50\u306e\u4f4d\u306b\u5c31\u304d\u3001\u30af\u30b9\u30b3\u306e\u5171\u540c\u6307\u5c0e\u8005\u3068\u306a\u3063\u305f\u3002"
            },
            {
                "heading": "\u5e1d\u56fd\u306e\u5efa\u8a2d",
                "text": "1438\u5e74\u3001\u7236\u304c\u901d\u53bb\u3057\u305f\u3053\u3068\u3067\u30af\u30b9\u30b3\u306e\u552f\u4e00\u306e\u6307\u5c0e\u8005\u3068\u306a\u3063\u305f\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u3001\u3059\u3050\u306b\u8fd1\u96a3\u8af8\u56fd\u3078\u306e\u4fb5\u653b\u3092\u958b\u59cb\u3057\u3001\u6210\u529f\u3055\u305b\u308b\u3002\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306e\u65b0\u5e1d\u56fd\u306f\u30a8\u30af\u30a2\u30c9\u30eb\u304b\u3089\u30c1\u30ea\u306b\u307e\u3067\u62e1\u5927\u3057\u3001\u5357\u30a2\u30e1\u30ea\u30ab\u306e\u5f37\u56fd\u306e1\u3064\u3068\u306a\u3063\u305f\u3002\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u6b66\u529b\u306b\u3088\u308a\u591a\u304f\u306e\u738b\u56fd\u3092\u7372\u5f97\u3057\u3066\u3044\u3063\u305f\u304c\u3001\u65b0\u305f\u306a\u5730\u57df\u3092\u624b\u306b\u5165\u308c\u308b\u969b\u306b\u306f\u3001\u3072\u3068\u7656\u3042\u308b\u65b9\u6cd5\u3092\u4f7f\u3046\u3053\u3068\u3082\u3042\u3063\u305f\u3002\u307e\u305a\u76ee\u7684\u306e\u5730\u57df\u3078\u30b9\u30d1\u30a4\u3092\u9001\u308a\u8fbc\u307f\u3001\u8cc7\u7523\u3084\u8ecd\u4e8b\u529b\u306a\u3069\u306e\u60c5\u5831\u3092\u5f97\u308b\u3002\u305d\u3057\u3066\u60c5\u5831\u306e\u5185\u5bb9\u304c\u826f\u3044\u3082\u306e\u3067\u3042\u3063\u305f\u5834\u5408\u306b\u306f\u3001\u305d\u306e\u5730\u57df\u306e\u6307\u5c0e\u8005\u305f\u3061\u3092\u62db\u5f85\u3057\u3001\u30a4\u30f3\u30ab\u306e\u6cd5\u306b\u5f93\u3048\u3070\u3044\u304b\u306b\u751f\u6d3b\u304c\u8c4a\u304b\u306b\u306a\u308b\u304b\u3092\u71b1\u5fc3\u306b\u8aac\u660e\u3057\u3001\u5e73\u548c\u88cf\u306b\u30a4\u30f3\u30ab\u306b\u5f93\u3046\u3088\u3046\u8aac\u5f97\u3057\u305f\u3002\u591a\u304f\u306e\u738b\u56fd\u306f(\u30c1\u30e3\u30f3\u30ab\u65cf\u306e\u4e8c\u306e\u821e\u306b\u306a\u308b\u3053\u3068\u3092\u6016\u308c)\u3053\u306e\u63d0\u6848\u306b\u5fdc\u3058\u3001\u30a4\u30f3\u30ab\u306e\u6cd5\u306e\u4e0b\u3067\u6559\u80b2\u3092\u65bd\u3059\u305f\u3081\u306b\u3001\u81ea\u5206\u306e\u5b50\u4f9b\u305f\u3061\u3092\u30af\u30b9\u30b3\u3078\u3068\u79fb\u4f4f\u3055\u305b\u305f\u3002\u30a4\u30f3\u30ab\u306e\u7fd2\u308f\u3057\u3092\u6559\u3048\u8fbc\u307e\u308c\u305f\u5b50\u4f9b\u305f\u3061\u306f\u30a4\u30f3\u30ab\u306e\u8cb4\u65cf\u305f\u3061\u3068\u7d50\u5a5a\u3057\u3001\u305d\u306e\u5f8c\u51fa\u8eab\u5730\u3078\u3068\u623b\u308a\u3001\u6545\u90f7\u3092\u6cbb\u3081\u305f\u3002\u3053\u3046\u3057\u3066\u5e1d\u56fd\u306e\u62e1\u5927\u3068\u5e73\u548c\u306f\u7dad\u6301\u3055\u308c\u7d9a\u3051\u305f\u3002\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u3001\u65b0\u305f\u306b\u7372\u5f97\u3057\u305f\u571f\u5730\u306e\u79e9\u5e8f\u3092\u7dad\u6301\u3059\u308b\u305f\u3081\u306b\u56fd\u30924\u3064\u306e\u5dde\u306b\u5206\u3051\u3001\u5404\u5dde\u306b\u9996\u9577\u3092\u7f6e\u3044\u3066\u901a\u5e38\u4efb\u52d9\u306b\u5f53\u305f\u3089\u305b\u305f\u3002\u307e\u305f\u8056\u8077\u90e8\u9580\u3068\u8ecd\u4e8b\u90e8\u9580\u3092\u76e3\u8996\u3059\u308b\u72ec\u7acb\u6a5f\u95a2\u3092\u8a2d\u7f6e\u3057\u3001\u6700\u53e4\u671f\u306e\u6291\u5236\u3068\u5747\u8861\u306e\u30b7\u30b9\u30c6\u30e0\u3092\u4f5c\u308a\u4e0a\u3052\u305f\u3002\u3055\u3089\u306b\u5e1d\u56fd\u306e\u9996\u90fd\u3068\u3057\u3066\u30af\u30b9\u30b3\u3092\u518d\u5efa\u3057\u3001\u5404\u5dde\u304b\u3089\u306e\u79fb\u6c11\u3092\u51fa\u8eab\u5730\u306b\u5bfe\u5fdc\u3059\u308b\u5730\u533a\u306b\u4f4f\u307e\u308f\u305b\u305f\u3002\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u3053\u306e\u9803\u3001\u6709\u540d\u306a\u30de\u30c1\u30e5\u30fb\u30d4\u30c1\u30e5\u3092\u5efa\u8a2d\u3057\u3066\u3044\u308b\u304c\u3001\u3053\u308c\u306f\u73fe\u5728\u3067\u306f\u3001\u79c1\u7684\u306a\u5c71\u306e\u5225\u8358\u3068\u3057\u3066\u5efa\u3066\u3089\u308c\u305f\u3082\u306e\u3060\u3068\u8a00\u308f\u308c\u3066\u3044\u308b\u30021471\u5e74\u306b\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u304c\u901d\u53bb\u3059\u308b\u3068\u3001\u6226\u58eb\u3068\u3057\u3066\u306e\u8cc7\u8cea\u306b\u6b20\u3051\u3066\u3044\u305f\u606f\u5b50\u30a2\u30de\u30eb\u306e\u5373\u4f4d\u306f\u898b\u9001\u3089\u308c\u3001\u305d\u306e\u5f1f\u306e\u30c8\u30a5\u30d1\u30c3\u30af\u304c\u30a4\u30f3\u30ab\u5e1d\u56fd\u306e\u65b0\u305f\u306a\u7687\u5e1d\u3068\u306a\u3063\u305f\u3002"
            },
            {
                "heading": "\u6b74\u53f2\u306e\u5be9\u5224",
                "text": "\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u30da\u30eb\u30fc\u306e\u56fd\u6c11\u7684\u82f1\u96c4\u3067\u3042\u308b\u3002\u307e\u305f\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u304c\u5e1d\u56fd\u5185\u306b\u5efa\u3066\u305f\u30e2\u30cb\u30e5\u30e1\u30f3\u30c8\u306e\u591a\u304f\u306f\u3001\u4eca\u3067\u3082\u5f53\u6642\u306e\u307e\u307e\u306e\u59ff\u3067\u898b\u308b\u3053\u3068\u304c\u3067\u304d\u308b\u3002\u653f\u6cbb\u3084\u8ecd\u4e8b\u306b\u9577\u3051\u3066\u3044\u305f\u3068\u3057\u3066\u77e5\u3089\u308c\u3066\u3044\u308b\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u3060\u304c\u3001\u305d\u306e\u6148\u60b2\u6df1\u3055\u306b\u3064\u3044\u3066\u306f\u7591\u554f\u7b26\u304c\u4ed8\u304f\u3002\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306f\u5e1d\u56fd\u3092\u7dad\u6301\u3059\u308b\u305f\u3081\u306b\u6570\u5341\u4e07\u306e\u4eba\u3005\u3092\u8ffd\u653e\u3057\u3001\u5e1d\u56fd\u5468\u8fba\u306e\u9069\u5f53\u306a\u5834\u6240\u306b\u79fb\u4f4f\u3055\u305b\u3066\u3044\u305f\u3002\u3057\u304b\u3057\u3001\u78ba\u304b\u306b\u554f\u984c\u306f\u3042\u3063\u305f\u3068\u306f\u3044\u3048\u3001\u30a4\u30f3\u30ab\u6700\u5927\u306e\u5f81\u670d\u306e\u6642\u4ee3\u3092\u958b\u59cb\u3057\u3001\u30a4\u30f3\u30ab\u304c\u3084\u304c\u3066\u5357\u30a2\u30e1\u30ea\u30ab\u306e\u6587\u660e\u56fd\u306e\u307b\u307c\u5168\u3066\u3092\u53d6\u308a\u8fbc\u3080\u307b\u3069\u306b\u9818\u571f\u3092\u62e1\u5927\u3059\u308b\u790e\u3092\u7bc9\u3044\u305f\u306e\u306f\u3001\u3053\u306e\u30d1\u30c1\u30e3\u30af\u30c6\u30a3\u306a\u306e\u3067\u3042\u308b\u3002"
            }
        ]
    }
}