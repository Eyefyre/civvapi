        "trait": {
            "name": "\u6355\u865c\u306e\u751f\u8d04",
            "effect": "\u6575\u30e6\u30cb\u30c3\u30c8\u3092\u5012\u3059\u3054\u3068\u306b\u6587\u5316\u529b\u3092\u7372\u5f97\u3059\u308b\u3002"
        },
        "titles": [
