        "icon": "https://eyefyre.github.io/civvapi/images/leader_icons/\u30cf\u30fc\u30eb\u30fc\u30f3\u30fb\u30a2\u30c3\u30e9\u30b7\u30fc\u30c9.png",
        "trait": {
            "name": "\u7802\u6f20\u306e\u8239",
            "effect": "\u968a\u5546\u306e\u5230\u9054\u53ef\u80fd\u8ddd\u96e2\u304c50%\u4f38\u3073\u308b\u3002\u6240\u6709\u3059\u308b\u90fd\u5e02\u304b\u3089\u51fa\u3066\u3044\u304f\u4ea4\u6613\u8def\u306f\u3001\u305d\u306e\u90fd\u5e02\u306e\u5b97\u6559\u3092\u5e83\u3081\u308b\u52b9\u679c\u30922\u500d\u306b\u3057\u3001[ICON_OIL]\u77f3\u6cb9\u8cc7\u6e90\u304c2\u500d\u306b\u306a\u308b\u3002"
        },
        "titles": [
            "\u30ab\u30ea\u30d5",
            "\u300c\u6b63\u3057\u304d\u9053\u3092\u6b69\u3080\u8005\u300d"
        ],
        "historical_info": [
            {
                "heading": "\u751f\u6daf",
                "text": "\u30cf\u30fc\u30eb\u30fc\u30f3\u30fb\u30a2\u30c3\u30e9\u30b7\u30fc\u30c9(\u300c\u6b63\u3057\u304f\u5c0e\u304b\u308c\u305f\u30a2\u30ed\u30f3\u300d\u306e\u610f)\u306f\u30a2\u30c3\u30d0\u30fc\u30b9\u671d\u7b2c5\u4ee3\u30ab\u30ea\u30d5\u3067\u3001\u7d00\u5143786\u5e74\u304b\u3089809\u5e74\u307e\u3067\u30a2\u30e9\u30d3\u30a2\u5e1d\u56fd\u3092\u7d71\u6cbb\u3057\u305f\u3002\u5728\u4f4d\u4e2d\u3001\u5f7c\u306e\u30ab\u30ea\u30d5\u3068\u3057\u3066\u306e\u5730\u4f4d\u306f\u897f\u306f\u30b9\u30da\u30a4\u30f3\u3001\u5317\u306f\u30a2\u30ca\u30c8\u30ea\u30a2\u3001\u6771\u306f\u30a4\u30f3\u30c9\u306b\u307e\u3067\u62e1\u5927\u3057\u3001\u4e16\u754c\u6700\u5927\u306b\u3057\u3066\u6700\u5f37\u306e\u56fd\u5bb6\u3067\u3042\u3063\u305f\u3002\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u7d71\u6cbb\u8005\u3068\u3057\u3066\u6709\u80fd\u3067\u3042\u308a\u3001\u5f7c\u306e\u6cbb\u4e16\u306f\u88ab\u652f\u914d\u6c11\u65cf\u306e\u79d1\u5b66\u7684\u304a\u3088\u3073\u6587\u5316\u7684\u767a\u5c55\u3068\u7e41\u6804\u306e\u6642\u4ee3\u3067\u3042\u3063\u305f\u3002"
            },
            {
                "heading": "\u7d71\u6cbb\u6642\u4ee3\u521d\u671f",
                "text": "\u7b2c3\u4ee3\u30ab\u30ea\u30d5\u3068\u30a4\u30a8\u30e1\u30f3\u4eba\u306e\u5974\u96b7\u30cf\u30a4\u30ba\u30e9\u30fc\u30f3\u3068\u306e\u606f\u5b50\u3067\u3042\u308b\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u3001\u5144\u3067\u3042\u308b\u30a2\u30d6\u30fb\u30a2\u30d6\u30c9\u30a5\u30e9\u30fb\u30e0\u30fc\u30b5\u30fb\u30a4\u30d6\u30f3\u30fb\u30de\u30cf\u30c7\u30a3\u30fb\u30cf\u30fc\u30c7\u30a3\u30fc\u306e\u6b7b\u5f8c\u3001\u7687\u5e1d\u306e\u5ea7\u306b\u5c31\u3044\u305f\u3002\u30cf\u30fc\u30c7\u30a3\u30fc\u306f\u8b0e\u3081\u3044\u305f\u72b6\u6cc1\u3067\u80c3\u8178\u969c\u5bb3\u3092\u5f15\u304d\u8d77\u3053\u3057\u3066\u6b7b\u4ea1\u3057\u3066\u304a\u308a\u3001\u6b21\u7537\u306e\u30cf\u30fc\u30eb\u30fc\u30f3\u306b\u5bfe\u3057\u3066\u5927\u304d\u306a\u5f71\u97ff\u529b\u3092\u6301\u3063\u3066\u3044\u305f\u6bcd\u304c\u30cf\u30fc\u30c7\u30a3\u30fc\u3092\u6bd2\u6bba\u3057\u305f\u3068\u8a00\u308f\u308c\u3066\u3044\u308b\u3002\u771f\u507d\u306f\u3069\u3046\u3067\u3042\u308c\u3001\u30cf\u30a4\u30ba\u30e9\u30fc\u30f3\u306f\u30cf\u30fc\u30eb\u30fc\u30f3\u304c789\u5e74\u306b\u6b7b\u53bb\u3059\u308b\u307e\u3067\u3001\u5f7c\u306e\u7b46\u982d\u9867\u554f\u306e1\u4eba\u3060\u3063\u305f\u3002"
            },
            {
                "heading": "\u30d0\u30b0\u30c0\u30fc\u30c9\u30fb\u30eb\u30cd\u30b5\u30f3\u30b9",
                "text": "\u30cf\u30fc\u30eb\u30fc\u30f3\u306e\u7d71\u6cbb\u304c\u59cb\u307e\u3063\u305f\u5f53\u6642\u3001\u30ab\u30ea\u30d5\u5bae\u5ef7\u306f\u5148\u4ee3\u306e\u30ab\u30ea\u30d5\u304c\u7bc9\u3044\u305f\u65b0\u3057\u3044\u90fd\u5e02\u30d0\u30b0\u30c0\u30fc\u30c9\u3067\u3042\u3063\u305f\u3002\u3053\u306e\u8857\u306f\u82b8\u8853\u3001\u79d1\u5b66\u305d\u3057\u3066\u5b97\u6559\u306e\u4e2d\u5fc3\u3067\u3042\u308a\u3001\u591a\u304f\u306e\u7f8e\u3057\u3044\u5efa\u7269\u304c\u5b58\u5728\u3057\u305f\u3002\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u305d\u3053\u306b\u300c\u77e5\u6075\u306e\u5bb6\u300d\u3068\u547c\u3070\u308c\u308b\u56f3\u66f8\u9928\u517c\u7814\u7a76\u65bd\u8a2d\u3092\u5efa\u8a2d\u3057\u3001\u30da\u30eb\u30b7\u30a2\u3001\u30a4\u30f3\u30c9\u3001\u30ae\u30ea\u30b7\u30e3\u3001\u30ed\u30fc\u30de\u306e\u79d1\u5b66\u6587\u732e\u306e\u53ce\u96c6\u3068\u7ffb\u8a33\u3092\u884c\u3063\u305f\u3002\u30cf\u30fc\u30eb\u30fc\u30f3\u306e\u7d71\u6cbb\u4e0b\u3067\u30d0\u30b0\u30c0\u30fc\u30c9\u306f\u7e41\u6804\u3057\u3001\u304a\u305d\u3089\u304f\u4e16\u754c\u6700\u5927\u306e\u88d5\u798f\u306a\u90fd\u5e02\u3068\u306a\u3063\u305f\u3002\u5f8c\u306b\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u653f\u5e9c\u6a5f\u95a2\u3092\u6226\u7565\u7684\u306b\u91cd\u8981\u306a\u90fd\u5e02\u30e9\u30c3\u30ab\u306b\u79fb\u3059\u304c\u3001\u30d0\u30b0\u30c0\u30fc\u30c9\u306f\u305d\u306e\u5f8c\u6570\u4e16\u7d00\u306b\u308f\u305f\u308a\u82b8\u8853\u3001\u79d1\u5b66\u3001\u5546\u696d\u306e\u4e00\u5927\u90fd\u5e02\u3068\u3057\u3066\u5b58\u7d9a\u3057\u305f(\u30d0\u30b0\u30c0\u30fc\u30c9\u306f1258\u5e74\u306b\u30e2\u30f3\u30b4\u30eb\u4eba\u306b\u3088\u308a\u5236\u5727\u3001\u7834\u58ca\u3055\u308c\u305f)\u3002"
            },
            {
                "heading": "\u30cf\u30fc\u30eb\u30fc\u30f3\u3068\u30a2\u30e9\u30d3\u30a2\u306e\u5bcc",
                "text": "\u30cf\u30fc\u30eb\u30fc\u30f3\u306b\u3064\u3044\u3066\u3001\u3069\u3053\u3068\u306a\u304f\u5e7b\u60f3\u7684\u306a\u8a18\u8ff0\u3092\u300c\u5343\u591c\u4e00\u591c\u7269\u8a9e\u300d\u306e\u4e2d\u3067\u76ee\u306b\u3059\u308b\u3053\u3068\u304c\u3067\u304d\u308b\u3002\u305d\u306e\u4e2d\u3067\u30ab\u30ea\u30d5\u306f\u3001\u91d1\u3001\u9280\u3001\u5b9d\u77f3\u304c\u3061\u308a\u3070\u3081\u3089\u308c\u305f\u8c6a\u83ef\u306a\u5bae\u6bbf\u306b\u4f4f\u3093\u3067\u3044\u305f\u3068\u66f8\u304b\u308c\u3066\u3044\u308b\u3002\u8a87\u5f35\u3055\u308c\u3066\u306f\u3044\u308b\u3082\u306e\u306e\u3001\u7269\u8a9e\u306b\u306f\u78ba\u304b\u306a\u4fe1\u6191\u6027\u3082\u3042\u308b\u3002\u30cf\u30fc\u30eb\u30fc\u30f3\u306e\u5728\u4f4d\u4e2d\u3001\u5e1d\u56fd\u306b\u306f\u5de8\u4e07\u306e\u5bcc\u304c\u6d41\u308c\u8fbc\u307f\u3001\u305d\u306e\u5927\u90e8\u5206\u306f\u30ab\u30ea\u30d5\u306e\u61d0\u306b\u5165\u3063\u3066\u3044\u305f\u3002\u53e4\u4ee3\u6b74\u53f2\u5b66\u8005\u306b\u3088\u308c\u3070\u3001\u30cf\u30fc\u30eb\u30fc\u30f3\u306e\u59bb\u306f\u98df\u5353\u306b\u4e26\u3076\u98df\u5668\u3092\u5168\u3066\u91d1\u88fd\u306b\u3057\u3001\u5b9d\u77f3\u3067\u98fe\u308b\u3088\u3046\u305b\u304c\u3093\u3060\u3068\u8a00\u3046\u3002"
            },
            {
                "heading": "\u5916\u4ea4\u95a2\u4fc2",
                "text": "\u653f\u6cbb\u7684\u306b\u8a00\u3048\u3070\u3001\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u30e8\u30fc\u30ed\u30c3\u30d1\u5217\u5f37\u3068\u306e\u89aa\u5584\u7684\u306a\u95a2\u4fc2\u3092\u7dad\u6301\u3057\u3088\u3046\u3068\u52aa\u3081\u305f\u3002\u5f7c\u306f\u30b7\u30e3\u30eb\u30eb\u30de\u30fc\u30cb\u30e5\u3068\u76f4\u63a5\u7684\u306a\u5916\u4ea4\u95a2\u4fc2\u3092\u6301\u3061\u3001\u5341\u5b57\u8ecd\u4ee5\u524d\u306e\u30e8\u30fc\u30ed\u30c3\u30d1\u4eba\u306f\u30a8\u30eb\u30b5\u30ec\u30e0\u3084\u8056\u5730\u3078\u81ea\u7531\u306b\u884c\u304d\u6765\u3057\u3066\u3044\u305f\u3002\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u4e2d\u56fd\u306e\u5bae\u5ef7\u3068\u3082\u5916\u4ea4\u95a2\u4fc2\u3092\u6301\u3063\u3066\u3044\u305f\u3002\u3057\u304b\u3057\u306a\u304c\u3089\u3001\u5f7c\u3068\u30d3\u30b6\u30f3\u30c1\u30f3\u3068\u306e\u95a2\u4fc2\u306f\u3042\u307e\u308a\u53cb\u597d\u7684\u306a\u3082\u306e\u3067\u306f\u306a\u304b\u3063\u305f\u3002\u7236\u306e\u4e0b\u3067\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u3001\u30c8\u30eb\u30b3\u3092\u901a\u904e\u3057\u3066\u30d3\u30b6\u30f3\u30c1\u30f3\u5e1d\u56fd\u306e\u9996\u90fd\u30b3\u30f3\u30b9\u30bf\u30f3\u30c6\u30a3\u30ce\u30fc\u30d7\u30eb\u3078\u3068\u8ecd\u968a\u3092\u7387\u3044\u305f\u3053\u3068\u304c\u3042\u308b\u3002\u5973\u5e1d\u30a4\u30ec\u30fc\u30cd\u3068\u4ea4\u6e09\u3057\u305f\u5f8c\u3001\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u91d1\u8ca87\u4e07\u679a\u306e\u5e74\u8ca2\u3068\u5f15\u304d\u63db\u3048\u306b\u9996\u90fd\u3092\u653b\u6483\u3057\u306a\u3044\u3053\u3068\u306b\u5408\u610f\u3057\u305f\u3002806\u5e74\u306b\u30a4\u30ec\u30fc\u30cd\u304c\u9000\u4f4d\u3059\u308b\u3068\u30d3\u30b6\u30f3\u30c1\u30f3\u306f\u3053\u306e\u5408\u610f\u3092\u7834\u308b\u3088\u3046\u306b\u306a\u308a\u3001\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u30d3\u30b6\u30f3\u30c1\u30f3\u3092\u529b\u305a\u304f\u3067\u964d\u4f0f\u3055\u305b\u308b\u305f\u3081\u306b\u518d\u3073\u5317\u3078\u3068\u8ecd\u968a\u3092\u7387\u3044\u305f\u3002"
            },
            {
                "heading": "\u30cf\u30fc\u30eb\u30fc\u30f3\u306e\u6b7b",
                "text": "808\u5e74\u3001\u30a4\u30e9\u30f3\u3067\u306e\u53cd\u4e71\u3092\u93ae\u3081\u308b\u65c5\u306b\u51fa\u305f\u9014\u4e2d\u3067\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u75c5\u306b\u5012\u308c\u3001\u3053\u306e\u4e16\u3092\u53bb\u3063\u305f\u3002\u5f7c\u306e\u606f\u5b50\u3067\u3042\u308b\u30a2\u30df\u30fc\u30f3\u304c\u738b\u4f4d\u3092\u7d99\u627f\u3057\u305f\u3002\u30cf\u30fc\u30eb\u30fc\u30f3\u306e\u6b7b\u53bb\u306b\u3088\u308a\u30a2\u30e9\u30d3\u30a2\u5e1d\u56fd\u306e\u7de9\u3084\u304b\u306a\u8870\u9000\u304c\u59cb\u307e\u308a\u3001\u56fd\u5916\u306e\u6575\u3084\u56fd\u5185\u306e\u53cd\u4e71\u306b\u3088\u3063\u3066\u5e1d\u56fd\u306f\u5c11\u3057\u305a\u3064\u9818\u571f\u3092\u5931\u3063\u3066\u3044\u3063\u305f\u3002\u5e1d\u56fd\u305d\u306e\u3082\u306e\u306f\u305d\u306e\u5f8c\u6570\u4e16\u7d00\u306b\u308f\u305f\u308a\u5b58\u7d9a\u3057\u305f\u304c\u3001\u30cf\u30fc\u30eb\u30fc\u30f3\u30fb\u30a2\u30c3\u30e9\u30b7\u30fc\u30c9\u306e\u7d71\u6cbb\u4e0b\u3067\u898b\u3089\u308c\u305f\u8f1d\u304b\u3057\u3044\u7d76\u9802\u671f\u306b\u53ca\u3076\u6642\u4ee3\u306f\u4e8c\u5ea6\u3068\u3084\u3063\u3066\u3053\u306a\u304b\u3063\u305f\u3002"
            },
            {
                "heading": "\u6b74\u53f2\u306e\u5be9\u5224",
                "text": "\u6226\u4e89\u3084\u5185\u7d1b\u304c\u3042\u3063\u305f\u306b\u3082\u304b\u304b\u308f\u3089\u305a\u3001\u30a2\u30c3\u30e9\u30b7\u30fc\u30c9\u306e\u6cbb\u4e16\u306f\u3001\u307b\u307c\u5e73\u548c\u3067\u7e41\u6804\u3057\u305f\u3082\u306e\u3067\u3042\u3063\u305f\u3002\u30ab\u30ea\u30d5\u306f\u7d4c\u6e08\u3068\u5de5\u696d\u306e\u767a\u9054\u306b\u52a0\u3048\u3001\u4ea4\u6613\u306e\u6025\u6210\u9577\u306b\u3088\u308b\u6069\u6075\u3092\u53d7\u3051\u305f\u3002\u30cf\u30fc\u30eb\u30fc\u30f3\u306f\u97f3\u697d\u3068\u8a69\u3092\u3053\u3088\u306a\u304f\u611b\u3057\u3001\u5bae\u5ef7\u306e\u82b8\u8853\u5bb6\u306b\u8d05\u6ca2\u306a\u8d08\u308a\u7269\u3092\u4e0e\u3048\u308b\u3053\u3068\u3082\u3042\u3063\u305f\u3002\u5fc5\u305a\u3057\u3082\u5049\u5927\u306a\u6307\u5c0e\u8005\u3067\u306f\u306a\u304b\u3063\u305f\u304c\u3001\u30a2\u30c3\u30e9\u30b7\u30fc\u30c9\u306f\u6a29\u529b\u3068\u5bcc\u306e\u7d76\u9802\u671f\u306b\u3042\u3063\u305f\u30a2\u30e9\u30d3\u30a2\u5e1d\u56fd\u3092\u7d71\u6cbb\u3057\u305f\u7687\u5e1d\u305d\u306e\u4eba\u306a\u306e\u3067\u3042\u308b\u3002"
            }
