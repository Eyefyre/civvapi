    "dawn_of_man": "\u795d\u798f\u548c\u656c\u610f\uff0c\u5c0a\u656c\u7684\u5973\u738b\u72c4\u591a\uff0c\u8fe6\u592a\u57fa\u738b\u570b\u7684\u50b3\u5947\u5275\u59cb\u4eba\u3002\u6839\u64da\u5049\u5927\u8a69\u4eba\u7dad\u5409\u723e\u7684\u8a71\u8a9e\u8a18\u8f09\uff0c\u60a8\u7684\u4e08\u592b\u963f\u514b\u723e\u5df4\u65af\u6b7b\u65bc\u60a8\u7684\u5144\u9577\u6cf0\u723e\u570b\u738b\u76ae\u683c\u99ac\u5229\u7fc1\u4e4b\u624b\uff0c\u96a8\u5f8c\u5247\u5ba3\u5e03\u963f\u514b\u723e\u5df4\u65af\u7684\u8ca1\u5bf6\u6b63\u5f0f\u96b8\u5c6c\u65bc\u60a8\u4e86\u3002\u56e0\u70ba\u64d4\u5fc3\u60a8\u7684\u5144\u5f1f\u6703\u89ac\u89a6\u9019\u5bcc\u88d5\u7684\u8ca1\u5bcc\uff0c\u60a8\u548c\u60a8\u7684\u540c\u80de\u822a\u884c\u524d\u5f80\u65b0\u5927\u9678\u3002\u62b5\u9054\u5317\u975e\u6d77\u5cb8\u5f8c\uff0c\u60a8\u5229\u7528\u7c21\u55ae\u7684\u725b\u76ae\u64cd\u7e31\u6b3a\u9a19\u4e86\u7576\u5730\u7684\u570b\u738b\uff0c\u70ba\u60a8\u7684\u65b0\u5bb6\u5e36\u4f86\u907c\u95ca\u7684\u5730\u57df\uff0c\u4e5f\u5c31\u662f\u672a\u4f86\u7684\u8fe6\u592a\u57fa\u738b\u570b\u3002\u8070\u660e\u53c8\u611b\u641e\u602a\u7684\u72c4\u591a\u5973\u738b\uff0c\u9019\u500b\u4e16\u754c\u6e34\u671b\u4e00\u500b\u64c1\u6709\u8070\u660e\u76f4\u89ba\u8207\u7cbe\u660e\u7684\u9818\u5c0e\u8005\uff0c\u80fd\u70ba\u5373\u5c07\u5230\u4f86\u7684\u98a8\u66b4\u63d0\u4f9b\u4e00\u500b\u5e87\u8b77\u6240\u3002\u60a8\u80fd\u5e36\u9818\u4eba\u6c11\u5efa\u7acb\u4e00\u500b\u65b0\u7684\u738b\u570b\uff0c\u8d85\u8d8a\u66fe\u7d93\u986f\u8d6b\u4e00\u6642\u7684\u8fe6\u592a\u57fa\u55ce\uff1f\u60a8\u80fd\u5efa\u8d77\u4e00\u500b\u7d93\u5f97\u4f4f\u6642\u9593\u8003\u9a57\u7684\u6587\u660e\u55ce\uff1f",
