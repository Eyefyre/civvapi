    "game_info": "\u904a\u6232\u65e9\u671f\u5feb\u901f\u53c8\u5f37\u529b\u7684\u9a0e\u4e58\u55ae\u4f4d\uff0c\u88ab\u9577\u77db\u5175\u514b\u5236\u3002\u50c5\u9650\u62dc\u5360\u5ead\u751f\u7522\u3002\u8a72\u55ae\u4f4d\u6bd4\u8d77\u6240\u66ff\u4ee3\u7684\u9a0e\u624b\uff0c\u64c1\u6709\u66f4\u9ad8\u7684\u6230\u9b25\u529b\uff0c\u9632\u79a6\u66f4\u5f37\uff0c\u653b\u64ca\u57ce\u5e02\u7684\u6548\u679c\u4e5f\u66f4\u597d\u3002",
    "historical_info": "\u5728\u6b77\u53f2\u4e0a\u6240\u6709\u7684\u91cd\u88dd\u7532\u9a0e\u5175\u4e4b\u4e2d\uff0c\u7f85\u99ac\u9435\u7532\u9a0e\u5175\u662f\u6230\u5834\u4e0a\u53ef\u6015\u7684\u5c0d\u624b\u3002\u6230\u99ac\u8eab\u62ab\u9396\u5b50\u7532\u6216\u93a7\u7532\uff0c\u9a0e\u58eb\u66f4\u8eab\u8457\u540c\u6a23\u61fe\u4eba\u7684\u5168\u5957\u9435\u7532\u3002\u6b64\u985e\u9a0e\u5175\u90e8\u968a\u7684\u4e3b\u8981\u7684\u4efb\u52d9\u4fbf\u662f\u7c89\u788e\u6b65\u5175\u9632\u7dda\u3002\u5728\u62dc\u5360\u5ead\u5e1d\u570b\u7684\u50b3\u5947\u6b77\u53f2\u4e2d\uff0c\u4ed6\u5011\u6700\u8457\u540d\u7684\u4fbf\u662f\u5927\u91cf\u63a1\u7528\u7f85\u99ac\u9435\u7532\u9a0e\u5175\uff0c\u5728\u5176\u897f\u65b9\u7684\u7f85\u99ac\u53cb\u90a6\u8870\u843d\u4e4b\u5f8c\u4fdd\u8b77\u5e1d\u570b\uff0c\u767c\u63ee\u4e86\u8209\u8db3\u8f15\u91cd\u7684\u4f5c\u7528\u3002",
    "strategy": "\u9435\u7532\u9a0e\u5175\u662f\u62dc\u5360\u5ead\u7684\u7279\u8272\u55ae\u4f4d\uff0c\u66ff\u4ee3\u9a0e\u58eb\u3002\u9435\u7532\u9a0e\u5175\u96d6\u7136\u901f\u5ea6\u6bd4\u9a0e\u624b\u6162\uff0c\u4f46\u529b\u91cf\u66f4\u5f37\uff0c\u9019\u4f7f\u5176\u6210\u70ba\u9a0e\u58eb\u51fa\u73fe\u4e4b\u524d\u6700\u53ef\u6015\u7684\u9a0e\u5175\u55ae\u4f4d\u4e4b\u4e00\u3002\u4ed6\u5011\u80fd\u5728\u653b\u64ca\u5f8c\u79fb\u52d5\u3002\u8207\u9a0e\u624b\u4e0d\u540c\uff0c\u4ed6\u5011\u53ef\u7372\u5f97\u9632\u79a6\u52a0\u6210\u3002\u4ed6\u5011\u5728\u653b\u64ca\u57ce\u5e02\u6642\u4e5f\u4e0d\u6703\u53d7\u5230\u904e\u591a\u7684\u6e1b\u76ca\u3002",
