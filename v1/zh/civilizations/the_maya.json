    "dawn_of_man": "\u9ad8\u8cb4\u7684\u5df4\u52a0\u723e\u4e8c\u4e16\uff0c\u773e\u795e\u937e\u611b\u4e4b\u5b50\u8207\u5e15\u502b\u514b\u516c\u6c11\u4e4b\u5e87\u8b77\u8005\uff0c\u60a8\u7684\u4eba\u6c11\u5728\u60a8\u9762\u524d\u5c48\u819d\u81e3\u670d\u3002\u5728\u53d7\u5230\u5468\u906d\u7af6\u722d\u5c0d\u624b\u7684\u6253\u64ca\u591a\u5e74\u5f8c\uff0c\u60a8\u5411\u60a8\u4eba\u6c11\u7684\u6575\u4eba\u767c\u8d77\u53cd\u64ca\uff0c\u5c07\u5176\u9996\u9818\u6bba\u5bb3\u737b\u796d\u4f5c\u70ba\u4fae\u8fb1\u60a8\u524d\u4efb\u57f7\u653f\u8005\u7684\u5831\u5fa9\u3002\u5e15\u502b\u514b\u7684\u69ae\u8000\u53ea\u80fd\u5728\u60a8\u667a\u6167\u7684\u9818\u5c0e\u4e0b\u518d\u6b21\u91cd\u73fe\uff0c\u56e0\u70ba\u60a8\u7cbe\u5fc3\u7b56\u5283\u4e86\u9f90\u5927\u7684\u57ce\u5e02\u91cd\u5efa\u5de5\u7a0b\uff0c\u5efa\u9020\u4e86\u4e00\u4e9b\u60a8\u7684\u4eba\u6c11\u4ee5\u53ca\u9019\u500b\u4e16\u754c\u5f9e\u672a\u898b\u904e\u7684\u6700\u5049\u5927\u7684\u53e4\u8e5f\u548c\u5efa\u7bc9\u3002\u986f\u8d6b\u7684\u570b\u738b\uff0c\u60a8\u7684\u4eba\u6c11\u518d\u6b21\u671f\u5f85\u6709\u60a8\u9818\u5c0e\u8207\u6559\u8aa8\u7684\u65e5\u5b50\u3002\u60a8\u6703\u50b3\u9054\u795e\u7684\u65e8\u610f\uff0c\u518d\u6b21\u5e36\u9818\u60a8\u66fe\u7d93\u5f15\u4ee5\u70ba\u50b2\u7684\u738b\u570b\u91cd\u8fd4\u985b\u5cf0\u55ce\uff1f\u60a8\u6703\u8c4e\u7acb\u65b0\u7684\u7d00\u5ff5\u7891\uff0c\u6c38\u9060\u9298\u8a18\u4eba\u6c11\u7684\u56de\u61b6\u55ce\uff1f\u60a8\u80fd\u5efa\u8d77\u4e00\u500b\u7d93\u5f97\u4f4f\u6642\u9593\u8003\u9a57\u7684\u6587\u660e\u55ce\uff1f",
