        {
            "heading": "\u6b74\u53f2\u521d\u671f:\u6700\u521d\u306e\u79fb\u4f4f\u8005",
            "text": "\u53e4\u4ee3\u306e\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u306b\u3064\u3044\u3066\u306f\u3044\u307e\u3060\u8b0e\u306e\u307e\u307e\u3060\u304c\u300c3\u3064\u306e\u4eee\u8aac\u306e\u5185\u306e\u3069\u308c\u304b\u304c\u3001\u3069\u3046\u3084\u3063\u3066\u4eba\u3005\u304c\u5cf6\u306b\u8fbf\u308a\u7740\u3044\u305f\u304b\u3092\u8aac\u660e\u3057\u3066\u3044\u308b\u306f\u305a\u3060\u300d\u3001\u3068\u3044\u3046\u70b9\u3067\u306f\u8003\u53e4\u5b66\u8005\u3068\u4eba\u985e\u5b66\u8005\u306e\u9593\u3067\u610f\u898b\u304c\u4e00\u81f4\u3057\u3066\u3044\u308b\u3002\u6025\u884c\u5217\u8eca\u30e2\u30c7\u30eb\u3068\u547c\u3070\u308c\u308b\u7b2c1\u306e\u4eee\u8aac\u3067\u306f\u3001\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u306f\u304a\u3088\u305d3,000\u5e74\u524d\u3001\u7d041,000\u5e74\u304b\u3051\u3066\u53f0\u6e7e\u304b\u3089\u79fb\u52d5\u3057\u3066\u304d\u305f\u3068\u3057\u3066\u3044\u308b\u3002\u79fb\u52d5\u306e\u6700\u4e2d\u306b\u51fa\u4f1a\u3063\u305f\u6c11\u65cf\u305f\u3061\u3068\u591a\u5c11\u63a5\u89e6\u304c\u3042\u3063\u305f\u305f\u3081\u3001\u73fe\u5728\u306e\u3088\u3046\u306a\u907a\u4f1d\u5b50\u7684\u7279\u5fb4\u3084\u8a00\u8a9e\u7684\u7279\u5fb4\u3092\u6301\u3064\u3088\u3046\u306b\u306a\u3063\u305f\u3068\u8aac\u660e\u3057\u3066\u3044\u308b\u3002 \u6b8b\u308a\u306e2\u3064\u306e\u4eee\u8aac\u3001\u3082\u3064\u308c\u305f\u5217\u30e2\u30c7\u30eb\u3068\u30b9\u30ed\u30fc\u30dc\u30fc\u30c8\u30e2\u30c7\u30eb\u3067\u306f\u3001\u3082\u3063\u3068\u9577\u3044\u6642\u9593\u3092\u304b\u3051\u3066\u79fb\u4f4f\u3092\u884c\u3063\u305f\u3068\u3057\u3066\u304a\u308a\u3001\u7279\u306b\u30e1\u30e9\u30cd\u30b7\u30a2\u4eba\u3092\u306f\u3058\u3081\u3068\u3059\u308b\u5404\u5730\u306e\u539f\u4f4f\u6c11\u305f\u3061\u3068\u3001\u76f8\u4e92\u306b\u6df1\u3044\u95a2\u308f\u308a\u5408\u3044\u3092\u6301\u3063\u305f\u3068\u3057\u3066\u3044\u308b\u3002\u897f\u30dd\u30ea\u30cd\u30b7\u30a2\u306b\u3069\u306e\u7a0b\u5ea6\u306e\u6642\u9593\u3092\u304b\u3051\u3066\u8fbf\u308a\u7740\u3044\u305f\u304b\u306b\u3064\u3044\u3066\u306f\u8af8\u8aac\u3042\u308b\u304c\u3001\u3069\u306e\u7d4c\u8def\u3092\u8fbf\u3063\u3066\u79fb\u52d5\u3092\u884c\u3063\u3066\u304d\u305f\u304b\u306b\u3064\u3044\u3066\u306f\u304b\u306a\u308a\u4fe1\u983c\u3067\u304d\u308b\u7a0b\u5ea6\u306b\u307e\u3067\u5206\u6790\u304c\u9032\u3093\u3067\u3044\u308b\u3002\u6700\u5317\u7aef\u306e\u30cf\u30ef\u30a4\u306b\u4eba\u3005\u304c\u8fbf\u308a\u7740\u3044\u305f\u306e\u306f\u7d00\u5143500\u5e74\u9803\u3067\u3001\u305d\u308c\u306b\u7d9a\u304f\u7d00\u51431000\u5e74\u9803\u3001\u30cb\u30e5\u30fc\u30b8\u30fc\u30e9\u30f3\u30c9\u306b\u4eba\u3005\u304c\u4f4f\u307f\u59cb\u3081\u305f\u3068\u3055\u308c\u3066\u3044\u308b\u3002\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u304c\u3044\u3064\u30a4\u30fc\u30b9\u30bf\u30fc\u5cf6\u3092\u767a\u898b\u3057\u305f\u304b\u306b\u3064\u3044\u3066\u306f\u3044\u307e\u3060\u8b70\u8ad6\u306e\u6700\u4e2d\u3067\u3042\u308b\u3002\u30a4\u30fc\u30b9\u30bf\u30fc\u5cf6\u306b\u4eba\u3005\u304c\u4f4f\u307f\u59cb\u3081\u305f\u306e\u306f\u3001\u8003\u53e4\u5b66\u7684\u8a3c\u62e0\u304b\u3089\u7d00\u5143300\u5e74\u304b\u3089500\u5e74\u306e\u9593\u3060\u3068\u3059\u308b\u5834\u5408\u3082\u3042\u308b\u304c\u3001 \u73fe\u5728\u3067\u306f800\u5e74\u9803\u3067\u3042\u308b\u3068\u3044\u3046\u610f\u898b\u304c\u4e3b\u6d41\u3068\u306a\u3063\u3066\u3044\u308b\u3002\u307e\u305f\u4e2d\u306b\u306f\u3001\u7d00\u51431200\u5e74\u306b\u306a\u3063\u3066\u3088\u3046\u3084\u304f\u4eba\u3005\u304c\u5b9a\u4f4f\u3057\u59cb\u3081\u305f\u3068\u3059\u308b\u79d1\u5b66\u8005\u3082\u3044\u308b\u3002"
        },
        {
            "heading": "\u822a\u6d77\u8853",
            "text": "\u30dd\u30ea\u30cd\u30b7\u30a2\u30fb\u30c8\u30e9\u30a4\u30a2\u30f3\u30b0\u30eb\u306f\u975e\u5e38\u306b\u5de8\u5927\u3067\u3042\u308a\u3001\u5404\u8fba\u306e\u9577\u3055\u306f\u305d\u308c\u305e\u308c\u7d044,400\u30ad\u30ed\u30e1\u30fc\u30c8\u30eb\u3082\u3042\u308b\u3002\u3053\u306e\u5e83\u3055\u306e\u5916\u6d0b\u3092\u822a\u884c\u3059\u308b\u306b\u306f\u3001\u975e\u5e38\u306b\u9ad8\u3044\u30ec\u30d9\u30eb\u306e\u6d77\u8ecd\u6280\u8853\u3068\u822a\u6d77\u8853\u304c\u8981\u6c42\u3055\u308c\u3001\u305d\u308c\u306b\u52a0\u3048\u3066\u52c7\u6c17\u3082\u5fc5\u9808\u3068\u306a\u308b\u3002\u822a\u6d77\u58eb\u306f\u822a\u6d77\u8853\u306e\u30a6\u30a7\u30a4\u30d5\u30a1\u30a4\u30f3\u30c7\u30a3\u30f3\u30b0\u3092\u7528\u3044\u3066\u3001\u7279\u5b9a\u306e\u661f\u3068\u91ce\u751f\u52d5\u7269\u306e\u52d5\u304d\u3001\u6c17\u5019\u306e\u79fb\u308a\u5909\u308f\u308a\u3001\u305d\u3057\u3066\u6ce2\u306e\u9032\u3080\u65b9\u5411\u306a\u3069\u3092\u8a18\u9332\u3057\u305f\u3002\u8a18\u9332\u3055\u308c\u305f\u60c5\u5831\u306f\u53e3\u982d\u3067\u65b0\u4eba\u306e\u822a\u6d77\u58eb\u305f\u3061\u306b\u4f1d\u3048\u3089\u308c\u3001\u9078\u3070\u308c\u305f\u5bb6\u7cfb\u306e\u9593\u3067\u53b3\u91cd\u306b\u5b88\u3089\u308c\u305f\u3002\u5f7c\u3089\u304c\u767a\u660e\u3057\u305f\u30ab\u30cc\u30fc\u7528\u306e\u8237\u5916\u6d6e\u6750\u3082\u3001\u5e83\u5927\u306a\u6d77\u3092\u5b89\u5168\u306b\u822a\u884c\u3059\u308b\u305f\u306e\u52a9\u3051\u3068\u306a\u3063\u3066\u3044\u305f\u3002\u3053\u3046\u3044\u3063\u305f\u6280\u8853\u3084\u822a\u6d77\u8853\u306e\u591a\u304f\u306f\u3001\u73fe\u5728\u3067\u3082\u3001\u30dd\u30ea\u30cd\u30b7\u30a2\u306e\u8239\u4e57\u308a\u305f\u3061\u306e\u9593\u3067\u4f7f\u7528\u3055\u308c\u3066\u3044\u308b\u3002"
        },
        {
            "heading": "\u30a2\u30e1\u30ea\u30ab\u3068\u306e\u7e4b\u304c\u308a",
            "text": "\u73fe\u5728\u306e\u30cf\u30ef\u30a4\u306e\u5cb8\u306b\u521d\u3081\u3066\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u304c\u8fbf\u308a\u7740\u3044\u305f\u306e\u306f\u3001\u7d00\u5143500\u5e74\u9803\u3060\u3068\u3055\u308c\u3066\u3044\u308b\u3002\u305f\u3060\u3057\u3001\u8003\u53e4\u5b66\u7684\u898b\u5730\u304b\u3089\u7d00\u5143300\u5e74\u9803\u3060\u3068\u3059\u308b\u610f\u898b\u3082\u3042\u308b\u3002\u3053\u306e\u6b74\u53f2\u521d\u671f\u306e\u79fb\u4f4f\u8005\u305f\u3061\u306f\u3001\u30cf\u30ef\u30a4\u306e\u897f\u65b9\u7d046,400\u30ad\u30ed\u30e1\u30fc\u30c8\u30eb\u306b\u3042\u308b\u30de\u30eb\u30b1\u30b5\u30b9\u8af8\u5cf6\u304b\u3089\u79fb\u52d5\u3057\u3066\u304d\u305f\u3068\u8003\u3048\u3089\u308c\u3066\u3044\u308b\u3002\u79fb\u52d5\u306e\u7b2c2\u9663\u306f\u300111\u4e16\u7d00\u9803\u306e\u30e9\u30c6\u30a4\u30a2\u5cf6\u3068\u30dc\u30e9\u30dc\u30e9\u5cf6\u304b\u3089\u306e\u3082\u306e\u3067\u3001\u30bf\u30d2\u30c1\u304b\u3089\u79fb\u52d5\u3057\u3066\u304d\u305f\u53ef\u80fd\u6027\u3082\u3042\u308b\u3002\u53e4\u4ee3\u30cf\u30ef\u30a4\u306e\u4f1d\u627f\u306b\u3001\u30d1\u30a2\u30aa\u3068\u547c\u3070\u308c\u308b\u30bf\u30d2\u30c1\u4eba\u306e\u5b50\u5b6b\u304c\u51fa\u3066\u304f\u308b\u3053\u3068\u304b\u3089\u3001\u30cf\u30ef\u30a4\u4eba\u306e\u7956\u5148\u306f\u30bf\u30d2\u30c1\u4eba\u3067\u3042\u308b\u3068\u3059\u308b\u8003\u53e4\u5b66\u8005\u3082\u3044\u308b\u3002\u3053\u306e\u8aac\u3092\u652f\u6301\u3059\u308b\u6b74\u53f2\u5bb6\u305f\u3061\u306f\u3001\u3053\u306e\u6642\u79fb\u4f4f\u3057\u3066\u304d\u305f\u30bf\u30d2\u30c1\u4eba\u306e\u4e2d\u306b\u306f\u4f4d\u306e\u9ad8\u3044\u9996\u9577\u305f\u3061\u3082\u591a\u6570\u304a\u308a\u3001\u6cd5\u5f8b\u306e\u30ab\u30d7\u3084\u4eba\u9593\u306b\u3088\u308b\u751f\u3051\u8d04\u306a\u3069\u306e\u7fd2\u6163\u3082\u540c\u6642\u306b\u4f1d\u3048\u3089\u308c\u305f\u3068\u3057\u3066\u3044\u308b\u3002\u307e\u305f\u3001\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u304c\u30a2\u30e1\u30ea\u30ab\u5927\u9678\u306b\u5230\u9054\u3057\u3001\u5f8c\u306b\u6bcd\u56fd\u306e\u5cf6\u3005\u306b\u30a2\u30e1\u30ea\u30ab\u306e\u52d5\u7269\u3084\u690d\u7269\u3092\u5e83\u3081\u305f\u3068\u3044\u3046\u8a3c\u62e0\u3082\u5b58\u5728\u3057\u3066\u3044\u308b\u3002\u4f8b\u3048\u3070\u3001\u897f\u6b27\u306e\u63a2\u691c\u5bb6\u304c\u521d\u3081\u3066\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u3068\u51fa\u4f1a\u3063\u305f\u3068\u304d\u3001\u30a2\u30e1\u30ea\u30ab\u5148\u4f4f\u6c11\u65cf\u306e\u690d\u7269\u3067\u3042\u308b\u30b5\u30c4\u30de\u30a4\u30e2\u304c\u5cf6\u4e2d\u306b\u7e41\u8302\u3057\u3066\u3044\u305f\u3068\u3044\u3046\u8a18\u9332\u304c\u6b8b\u3063\u3066\u3044\u308b\u3002"
        },
        {
            "heading": "\u30e8\u30fc\u30ed\u30c3\u30d1\u4eba\u306e\u5230\u6765",
            "text": "\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u306f\u897f\u6b27\u8af8\u56fd\u3068\u51fa\u4f1a\u3046\u307e\u3067\u3001\u4ed6\u306e\u6587\u660e\u304b\u3089\u307b\u3068\u3093\u3069\u9694\u96e2\u3055\u308c\u305f\u72b6\u614b\u306b\u306a\u3063\u3066\u304a\u308a\u3001\u65b0\u305f\u306a\u571f\u5730\u3084\u4ea4\u6613\u8def\u3092\u6a21\u7d22\u3057\u3066\u3044\u308b\u6700\u4e2d\u3060\u3063\u305f\u30021500\u5e74\u4ee3\u534a\u3070\u306b\u306f\u3001\u5cf6\u3005\u306e\u591a\u304f\u304c\u65c5\u884c\u8005\u3084\u5ba3\u6559\u5e2b\u306a\u3069\u306e\u897f\u6b27\u4eba\u3068\u63a5\u89e6\u3092\u6301\u3064\u3088\u3046\u306b\u306a\u3063\u3066\u3044\u305f\u3002\u3057\u304b\u30571800\u5e74\u4ee3\u306b\u5165\u308b\u3068\u3001\u690d\u6c11\u5730\u653f\u7b56\u3092\u63a8\u3057\u9032\u3081\u308b\u30d5\u30e9\u30f3\u30b9\u3001\u30a4\u30ae\u30ea\u30b9\u3001\u30a2\u30e1\u30ea\u30ab\u306a\u3069\u304c\u6709\u4eba\u5cf6\u3092\u529b\u305a\u304f\u3067\u690d\u6c11\u5730\u306b\u3057\u3001 \u5cf6\u3005\u3092\u81ea\u3089\u306e\u4fdd\u8b77\u56fd\u3084\u5e1d\u56fd\u9818\u306b\u3057\u3066\u3044\u3063\u305f\u3002\u3053\u306e\u3068\u304d\u72ec\u7acb\u3092\u5b88\u308a\u5207\u3063\u305f\u306e\u306f\u3001\u30cf\u30ef\u30a4\u3092\u306f\u3058\u3081\u3068\u3059\u308b\u3054\u304f\u5c11\u6570\u306e\u5cf6\u3005\u3060\u3051\u3067\u3042\u3063\u305f\u3002"
        },
        {
            "heading": "\u73fe\u5728\u3068\u672a\u6765",
            "text": "\u5e83\u3044\u7bc4\u56f2\u306b\u308f\u305f\u3063\u3066\u69d8\u3005\u306a\u5cf6\u304c\u6563\u3089\u3070\u3063\u3066\u3044\u308b\u304c\u3001\u4eca\u3067\u3082\u539f\u4f4f\u6c11\u306e\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u305f\u3061\u306f\u4f3c\u305f\u3088\u3046\u306a\u6587\u5316\u7684\u7fd2\u6163\u3084\u4fe1\u4ef0\u3092\u5171\u6709\u3057\u3001\u540c\u3058\u8a00\u8a9e\u3092\u4f7f\u7528\u3057\u3066\u3044\u308b\u3002\u6b8b\u5ff5\u306a\u3053\u3068\u306b\u3001\u30de\u30aa\u30ea\u3092\u306f\u3058\u3081\u3068\u3059\u308b\u5148\u4f4f\u6c11\u65cf\u306e\u591a\u304f\u304c\u5c11\u6570\u6c11\u65cf\u306b\u306a\u3063\u3066\u304a\u308a\u3001\u305d\u306e\u4ee3\u308f\u308a\u306b\u897f\u6b27\u306e\u5b50\u5b6b\u305f\u3061\u304c\u4eba\u53e3\u306e\u5927\u534a\u3092\u5360\u3081\u308b\u3088\u3046\u306b\u306a\u3063\u3066\u3044\u308b\u3002\u3057\u304b\u3057\u3001\u5cf6\u3005\u306b\u4f4f\u3080\u539f\u4f4f\u6c11\u306e\u5927\u534a\u304c\u81ea\u5206\u305f\u3061\u306e\u6587\u5316\u3084\u8a00\u8449\u3092\u6b8b\u3059\u305f\u3081\u306b\u5c3d\u529b\u3057\u3066\u304a\u308a\u3001\u4e3b\u8981\u306a\u5730\u57df\u306e\u591a\u304f\u3067\u306f\u3001\u4f1d\u7d71\u306e\u518d\u751f\u3084\u6d78\u900f\u304c\u7cbe\u529b\u7684\u304b\u3064\u52b9\u679c\u7684\u306b\u5b9f\u65bd\u3055\u308c\u3066\u3044\u308b\u3002"
        },
        {
            "heading": "\u73fe\u5728\u3068\u672a\u6765",
            "text": "\u5e83\u3044\u7bc4\u56f2\u306b\u308f\u305f\u3063\u3066\u69d8\u3005\u306a\u5cf6\u304c\u6563\u3089\u3070\u3063\u3066\u3044\u308b\u304c\u3001\u4eca\u3067\u3082\u539f\u4f4f\u6c11\u306e\u30dd\u30ea\u30cd\u30b7\u30a2\u4eba\u305f\u3061\u306f\u4f3c\u305f\u3088\u3046\u306a\u6587\u5316\u7684\u7fd2\u6163\u3084\u4fe1\u4ef0\u3092\u5171\u6709\u3057\u3001\u540c\u3058\u8a00\u8a9e\u3092\u4f7f\u7528\u3057\u3066\u3044\u308b\u3002\u6b8b\u5ff5\u306a\u3053\u3068\u306b\u3001\u30de\u30aa\u30ea\u3092\u306f\u3058\u3081\u3068\u3059\u308b\u5148\u4f4f\u6c11\u65cf\u306e\u591a\u304f\u304c\u5c11\u6570\u6c11\u65cf\u306b\u306a\u3063\u3066\u304a\u308a\u3001\u305d\u306e\u4ee3\u308f\u308a\u306b\u897f\u6b27\u306e\u5b50\u5b6b\u305f\u3061\u304c\u4eba\u53e3\u306e\u5927\u534a\u3092\u5360\u3081\u308b\u3088\u3046\u306b\u306a\u3063\u3066\u3044\u308b\u3002\u3057\u304b\u3057\u3001\u5cf6\u3005\u306b\u4f4f\u3080\u539f\u4f4f\u6c11\u306e\u5927\u534a\u304c\u81ea\u5206\u305f\u3061\u306e\u6587\u5316\u3084\u8a00\u8449\u3092\u6b8b\u3059\u305f\u3081\u306b\u5c3d\u529b\u3057\u3066\u304a\u308a\u3001\u4e3b\u8981\u306a\u5730\u57df\u306e\u591a\u304f\u3067\u306f\u3001\u4f1d\u7d71\u306e\u518d\u751f\u3084\u6d78\u900f\u304c\u7cbe\u529b\u7684\u304b\u3064\u52b9\u679c\u7684\u306b\u5b9f\u65bd\u3055\u308c\u3066\u3044\u308b\u3002"
        }
    ],
