    "game_info": "\u6bcf\u8655\u5df2\u88ab\u8a72\u57ce\u5e02\u958b\u767c\u7684\u9ec3\u91d1\u548c\u767d\u9280\u8cc7\u6e90+2\u91d1\u5e63\u3002\u57ce\u5e02\u5fc5\u9808\u4f4d\u65bc\u81f3\u5c11\u5176\u4e2d\u4e00\u7a2e\u7926\u7522\u8cc7\u6e90\u9644\u8fd1\u3002",
    "strategy": "\u9444\u5e63\u5ee0\u53ea\u80fd\u5efa\u9020\u5728\u9644\u8fd1\u6709\u6539\u5584\u5f8c\u91d1\u7926\u6216\u9280\u7926\u8cc7\u6e90\u7684\u57ce\u5e02\u3002\u9444\u5e63\u5ee0\u6703\u589e\u52a0\u6b64\u57ce\u5e02\u6240\u80fd\u53d6\u5f97\u7684\u7926\u7522\u8cc7\u6e90\u4e4b\u91d1\u5e63\u7522\u51fa\u3002",
    "historical_info": "\u9444\u5e63\u5ee0\u662f\u4e00\u500b\u6587\u660e\u88fd\u9020\u8ca8\u5e63\u7684\u6a5f\u69cb\u3002\u904e\u53bb\u901a\u5e38\u4f7f\u7528\u91d1\u5c6c\u786c\u5e63\uff0c\u5230\u4e86\u73fe\u4ee3\u793e\u6703\u4e16\u754c\u5247\u5927\u591a\u6578\u6539\u7528\u7d19\u9214\u3002\u9444\u5e63\u5ee0\u6538\u95dc\u8457\u4e00\u500b\u6587\u660e\u7d93\u6fdf\u6b63\u5e38\u904b\u4f5c\u8207\u5426\uff0c\u5982\u679c\u4e00\u500b\u570b\u5bb6\u7684\u8ca8\u5e63\u80fd\u8f15\u6613\u5c31\u88ab\u507d\u9020\uff0c\u4eba\u6c11\u5c0d\u8ca8\u5e63\u4f9b\u61c9\u7684\u4fe1\u5fc3\u4e5f\u6703\u4e0b\u964d\uff0c\u7d50\u679c\u5c31\u9020\u6210\u5371\u96aa\u7684\u901a\u8ca8\u81a8\u8139\uff08\u8ca8\u5e63\u5be6\u969b\u50f9\u503c\u5d29\u76e4\uff09\u751a\u81f3\u66f4\u7cdf\u7684\u60c5\u6cc1\u3002\u73fe\u4ee3\u7d19\u9214\u63a1\u7528\u5168\u606f\u6280\u8853\u3001\u8b8a\u8272\u6cb9\u58a8\u4e26\u52a0\u4e0a\u9632\u507d\u7dda\u4f86\u904f\u6b62\u507d\u9020\uff0c\u4f46\u5982\u679c\u88ab\u507d\u9020\u8005\u7834\u89e3\u7684\u8a71\uff0c\u52e2\u5fc5\u9084\u6703\u5f15\u5165\u65b0\u7684\u6280\u8853\u3002",
