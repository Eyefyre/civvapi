            "heading": "\u6b74\u53f2",
            "text": "\u30d5\u30f3\u65cf\u306f\u30014\u4e16\u7d00\u304b\u30895\u4e16\u7d00\u306b\u304b\u3051\u3066\u30e8\u30fc\u30ed\u30c3\u30d1\u5168\u57df\u306b\u79fb\u4f4f\u3057\u305f\u904a\u7267\u306e\u6c11\u3067\u3042\u308b\u3002\u30d5\u30f3\u65cf\u306f\u4e2d\u592e\u30a2\u30b8\u30a2\u306e\u8349\u539f\u5730\u5e2f\u3092\u6545\u90f7\u3068\u3059\u308b\u90e8\u65cf\u6c11\u3067\u3042\u3063\u305f\u3068\u8003\u3048\u3089\u308c\u308b\u3002\u30d5\u30f3\u65cf\u306e\u8ecd\u52e2\u306f\u3001\u6700\u5927\u306e\u6307\u5c0e\u8005\u30a2\u30c3\u30c6\u30a3\u30e9\u306e\u53f7\u4ee4\u306e\u4e0b\u3001\u30e8\u30fc\u30ed\u30c3\u30d1\u5168\u571f\u3067\u72fc\u85c9\u3068\u7565\u596a\u3092\u6b32\u3057\u3044\u307e\u307e\u306b\u3057\u3001\u30ed\u30fc\u30de\u3068\u305d\u306e\u540c\u76df\u8af8\u56fd\u3092\u6050\u6016\u306b\u9707\u3048\u4e0a\u304c\u3089\u305b\u305f\u3002"
        },
        {
            "heading": "\u6c17\u5019\u3068\u5730\u52e2",
            "text": "\u30d5\u30f3\u65cf\u306f\u30e2\u30f3\u30b4\u30eb\u5730\u65b9\u304a\u3088\u3073\u4e2d\u592e\u30a2\u30b8\u30a2\u304b\u3089\u79fb\u4f4f\u3057\u3066\u304d\u305f\u3068\u3055\u308c\u308b\u3002\u30d5\u30f3\u65cf\u304c\u521d\u3081\u3066\u30f4\u30a9\u30eb\u30ac\u5ddd\u3092\u6e21\u3063\u305f\u306e\u306f\u30014\u4e16\u7d00\u672b\u306e\u3053\u3068\u3067\u3042\u308b\u3002\u5f7c\u3089\u306e\u6545\u90f7\u306f\u5730\u7406\u7684\u591a\u69d8\u6027\u306b\u3042\u3075\u308c\u3001\u6a19\u9ad8\u304c\u9ad8\u304f\u51f9\u51f8\u306e\u591a\u3044\u30e2\u30f3\u30b4\u30eb\u5730\u65b9\u306e\u5e73\u539f\u3092\u3001\u7dd1\u306e\u5c71\u3005\u3001\u8349\u539f\u3001\u7802\u6f20\u3001\u305d\u3057\u3066\u5e7e\u767e\u3082\u306e\u5c0f\u3055\u306a\u6e56\u3084\u6e7f\u5730\u304c\u53d6\u308a\u56f2\u3093\u3067\u3044\u305f\u3002\u3053\u306e\u5730\u65b9\u306f\u91ce\u751f\u99ac\u306e\u54c1\u7a2e\u306b\u5bcc\u307f\u3001\u305d\u3053\u304b\u3089\u30d5\u30f3\u65cf\u306e\u5353\u8d8a\u3057\u305f\u99ac\u8853\u304c\u751f\u307e\u308c\u305f\u3053\u3068\u306f\u9593\u9055\u3044\u306a\u3044\u3002\u6700\u76db\u671f\u306b\u306f\u3001\u30d5\u30f3\u65cf\u306f\u4e2d\u592e\u30e8\u30fc\u30ed\u30c3\u30d1\u306e\u5e83\u3044\u5730\u57df\u3092\u652f\u914d\u3057\u3001\u4e2d\u592e\u30a2\u30b8\u30a2\u306e\u8349\u539f\u5730\u5e2f\u304b\u3089\u73fe\u5728\u306e\u30c9\u30a4\u30c4\u306b\u81f3\u308b\u5927\u5e1d\u56fd\u3092\u7bc9\u3044\u305f\u3002\u5730\u57df\u306b\u3088\u3063\u3066\u5730\u7406\u3084\u6c17\u5019\u304c\u3055\u307e\u3056\u307e\u3067\u3042\u3063\u305f\u304c\u3001\u5e83\u5927\u306a\u56fd\u571f\u3092\u7d71\u6cbb\u3059\u308b\u30d5\u30f3\u65cf\u306e\u80fd\u529b\u306f\u3001\u305d\u306e\u5f71\u97ff\u3092\u307b\u3068\u3093\u3069\u53d7\u3051\u306a\u304b\u3063\u305f\u3088\u3046\u3060\u3002"
        },
        {
            "heading": "\u30d5\u30f3\u65cf\u306e\u8d77\u6e90",
            "text": "\u30d5\u30f3\u65cf\u306f\u4e00\u4f53\u3069\u3053\u304b\u3089\u3084\u3063\u3066\u304d\u305f\u306e\u304b\uff1f\u3000\u71b1\u3044\u8b70\u8ad6\u3092\u5dfb\u304d\u8d77\u3053\u3057\u3066\u304d\u305f\u3053\u306e\u7591\u554f\u306b\u306f\u3001\u3044\u307e\u3060\u306b\u7b54\u3048\u304c\u51fa\u3066\u3044\u306a\u3044\u3002\u5927\u65b9\u306e\u63a8\u6e2c\u306b\u3088\u308c\u3070\u3001\u30d5\u30f3\u65cf\u306f\u300c\u3069\u3053\u304b\u3089\u3068\u3082\u306a\u304f\u73fe\u308c\u300d\u624b\u5f53\u305f\u308a\u3057\u3060\u3044\u306b\u7834\u58ca\u3092\u7e70\u308a\u8fd4\u3057\u305f\u3068\u3044\u3046\u3053\u3068\u306b\u306a\u308b\u3002\u6700\u3082\u4fe1\u6191\u6027\u304c\u9ad8\u3044\u306e\u306f\u3001\u4e2d\u56fd\u306e\u5317\u65b9\u3067\u8a95\u751f\u3057\u305f\u904a\u7267\u9a0e\u99ac\u6c11\u65cf\u306e\u90e8\u65cf\u9023\u5408\u3001\u5308\u5974\u306e\u4e00\u6d3e\u3067\u3042\u308b\u3068\u3044\u3046\u8aac\u3060\u3002\u5308\u5974\u306f\u7d00\u5143\u524d3\u4e16\u7d00\u3001\u5f53\u6642\u4e2d\u56fd\u3092\u652f\u914d\u3057\u3066\u3044\u305f\u79e6\u671d\u306b\u4fb5\u5165\u3092\u7e70\u308a\u8fd4\u3057\u3001\u904a\u7267\u6c11\u65cf\u306e\u4f8b\u306b\u3082\u308c\u305a\u3001\u4e07\u91cc\u306e\u9577\u57ce\u5efa\u8a2d\u306e\u4e00\u56e0\u3068\u306a\u3063\u305f\u3002\u30d5\u30f3\u65cf\u304c\u30e8\u30fc\u30ed\u30c3\u30d1\u306b\u59ff\u3092\u73fe\u3057\u305f\u306e\u306f\u3001\u3061\u3087\u3046\u3069\u5308\u5974\u304c\u59ff\u3092\u6d88\u3057\u305f\u6642\u671f\u3068\u4e00\u81f4\u3059\u308b\u3002\u6771\u65b9\u3067\u3042\u308b\u96c6\u56e3\u304c\u59ff\u3092\u6d88\u3059\u3068\u540c\u6642\u306b\u3001\u897f\u65b9\u306b\u672a\u77e5\u306e\u96c6\u56e3\u304c\u7a81\u5982\u51fa\u73fe\u3057\u305f\u3068\u3044\u3046\u3053\u3068\u306f\u3001\u30d5\u30f3\u65cf\u3068\u5308\u5974\u3092\u7d50\u3073\u3064\u3051\u308b\u6570\u5c11\u306a\u3044\u624b\u304c\u304b\u308a\u306e1\u3064\u3068\u8003\u3048\u3089\u308c\u3066\u3044\u308b\u3002"
        },
        {
            "heading": "\u30e8\u30fc\u30ed\u30c3\u30d1\u3078\u306e\u4fb5\u5165",
            "text": "\u30d5\u30f3\u65cf\u306b\u3064\u3044\u3066\u306e\u6bd4\u8f03\u7684\u53e4\u3044\u8a18\u9332\u3067\u306f\u3001\u3044\u305a\u308c\u3082\u6575\u306e\u8996\u70b9\u304b\u3089\u66f8\u304b\u308c\u305f\u3082\u306e\u3060\u3051\u306b\u3001\u516c\u5e73\u3068\u306f\u8a00\u3044\u304c\u305f\u3044\u3002\u3055\u3089\u306b\u3001\u30d5\u30f3\u65cf\u306b\u3088\u308b\u521d\u671f\u306e\u5f81\u670d\u306e\u6b74\u53f2\u306b\u3064\u3044\u3066\u306f\u7a7a\u767d\u304c\u591a\u3044\u30024\u4e16\u7d00\u306b\u30e8\u30fc\u30ed\u30c3\u30d1\u306b\u4fb5\u5165\u3057\u305f\u30d5\u30f3\u65cf\u306f\u3001\u30a2\u30e9\u30f3\u65cf\u3001\u6771\u30b4\u30fc\u30c8\u65cf\u3001\u897f\u30b4\u30fc\u30c8\u65cf\u3068\u3044\u3063\u305f\u7af6\u4e89\u76f8\u624b\u3092\u6b21\u3005\u3068\u670d\u5c5e\u3055\u305b\u305f\u3068\u8a00\u308f\u308c\u3066\u3044\u308b\u3002\u3053\u306e\u6642\u671f\u306e\u30d5\u30f3\u65cf\u306f\u307e\u3060\u7d71\u5236\u304c\u7de9\u304f\u3001\u4e00\u65b9\u3067\u306f\u6771\u897f\u30ed\u30fc\u30de\u306e\u9818\u571f\u3092\u5927\u80c6\u306b\u653b\u6483\u3057\u3066\u304a\u304d\u306a\u304c\u3089\u3001\u4ed6\u65b9\u3067\u306f\u30ed\u30fc\u30de\u306e\u50ad\u5175\u3068\u3057\u3066\u4ed6\u306e\u90e8\u65cf\u3068\u6226\u3046\u306a\u3069\u3057\u3066\u304a\u308a\u3001\u7bc0\u64cd\u304c\u306a\u304b\u3063\u305f\u3002\u30d3\u30b6\u30f3\u30c1\u30f3\u5e1d\u56fd(\u6771\u30ed\u30fc\u30de\u5e1d\u56fd)\u306f\u3053\u306e\u9803\u3001\u30d5\u30f3\u65cf\u3068\u306e\u878d\u548c\u3092\u306f\u304b\u308b\u305f\u3081\u306b\u8ca2\u7d0d\u91d1\u3092\u652f\u6255\u3046\u3088\u3046\u306b\u306a\u3063\u3066\u3044\u305f\u3002\u3057\u304b\u3057\u30ed\u30fc\u30de\u4eba\u305f\u3061\u306f\u3001\u3057\u3070\u3057\u3070\u3053\u306e\u7d04\u675f\u3092\u305f\u304c\u3048\u305f\u3002\u30a2\u30c3\u30c6\u30a3\u30e9\u306e\u7d71\u6cbb\u304c\u59cb\u307e\u308b\u76f4\u524d\u307e\u3067\u6642\u4ee3\u304c\u4e0b\u308b\u3068\u3001\u30d5\u30f3\u65cf\u306e\u6b74\u53f2\u306f\u3088\u308a\u9bae\u660e\u306b\u306a\u308b\u3002\u30a2\u30c3\u30c6\u30a3\u30e9\u306e\u4f2f\u7236\u306b\u3042\u305f\u308b\u30eb\u30fc\u30a2\u306f\u3001\u8af8\u90e8\u65cf\u304c\u5165\u308a\u6df7\u3058\u3063\u305f\u30d5\u30f3\u65cf\u306e\u8ecd\u52e2\u3092\u5f90\u3005\u306b\u638c\u63e1\u3057\u3001\u6575\u5bfe\u3059\u308b\u6307\u5c0e\u8005\u305f\u3061\u3092\u8ed2\u4e26\u307f\u7c9b\u6e05\u3057\u3066\u652f\u914d\u3092\u56fa\u3081\u305f\u3002\u30eb\u30fc\u30a2\u306f\u30d5\u30f3\u65cf\u3092\u5f37\u6a29\u7684\u306b\u652f\u914d\u3057\u3001\u8fd1\u96a3\u306e\u90e8\u65cf\u3092\u653b\u6483\u3057\u3066\u306f\u6210\u529f\u3092\u53ce\u3081\u3001\u3055\u3089\u306b\u306f\u3088\u308a\u5927\u304d\u306a\u6a19\u7684\u306b\u7167\u6e96\u3092\u5b9a\u3081\u308b\u3002\u300c\u30d5\u30f3\u90e8\u65cf\u9023\u5408\u306e\u738b\u300d(\u3053\u306e\u6642\u671f\u305d\u3046\u547c\u3070\u308c\u308b\u3088\u3046\u306b\u306a\u3063\u305f)\u3068\u3057\u3066\u7d71\u6cbb\u3057\u306a\u304c\u3089\u3001\u30eb\u30fc\u30a2\u306f\u6771\u30ed\u30fc\u30de\u5e1d\u56fd\u3068\u305f\u3073\u305f\u3073\u6226\u7aef\u3092\u958b\u3044\u305f\u3002\u3059\u3067\u306b\u30d5\u30f3\u65cf\u306b\u670d\u5f93\u3057\u3066\u3044\u305f\u8af8\u90e8\u65cf\u306e\u4e2d\u306b\u3001\u5e87\u8b77\u3092\u6c42\u3081\u3066\u30d3\u30b6\u30f3\u30c1\u30f3\u5e1d\u56fd\u306b\u9003\u3052\u8fbc\u3093\u3060\u90e8\u65cf\u304c\u3042\u3063\u305f\u3089\u3057\u3044\u3002\u30eb\u30fc\u30a2\u306f\u5f7c\u3089\u306e\u5f15\u304d\u6e21\u3057\u3092\u6c42\u3081\u3066\u30d3\u30b6\u30f3\u30c1\u30f3\u9818\u306b\u5175\u3092\u5c55\u958b\u3057\u3001\u30b3\u30f3\u30b9\u30bf\u30f3\u30c6\u30a3\u30ce\u30fc\u30d7\u30eb\u306e\u5317\u897f\u306b\u4f4d\u7f6e\u3059\u308b\u30c8\u30e9\u30ad\u30a2\u3092\u7565\u596a\u3057\u305f\u3002\u3068\u3053\u308d\u304c\u3001\u3053\u306e\u9060\u5f81\u306e\u3055\u306a\u304b\u306b\u30eb\u30fc\u30a2\u304c\u6025\u6b7b\u3057(\u96f7\u306b\u6483\u305f\u308c\u3066\u6b7b\u4ea1\u3057\u305f\u3068\u306e\u8aac\u304c\u3042\u308b)\u3001\u30d5\u30f3\u65cf\u306f\u64a4\u9000\u3092\u4f59\u5100\u306a\u304f\u3055\u308c\u305f\u3002"
        },
        {
            "heading": "\u6050\u6016\u306e\u8ecd\u52e2",
            "text": "434\u5e74\u306b\u30eb\u30fc\u30a2\u304c\u6b7b\u4ea1\u3059\u308b\u3068\u3001\u30a2\u30c3\u30c6\u30a3\u30e9\u3068\u305d\u306e\u5144\u30d6\u30ec\u30c0\u304c\u738b\u4f4d\u3092\u5206\u3051\u5408\u3044\u3001\u30e8\u30fc\u30ed\u30c3\u30d1\u306b\u304a\u3051\u308b\u30d5\u30f3\u65cf\u306e\u9818\u571f\u62e1\u5927\u3092\u7d99\u7d9a\u3059\u308b\u3002\u30d5\u30f3\u65cf\u306f\u3001\u7740\u5b9f\u306b\u5165\u3063\u3066\u304f\u308b\u8ca2\u7d0d\u91d1\u3068\u7565\u596a\u54c1\u3092\u4e3b\u306a\u8cc7\u91d1\u6e90\u3068\u3057\u30015\u4e16\u7d00\u306e\u30e8\u30fc\u30ed\u30c3\u30d1\u5927\u9678\u306b\u304a\u3044\u3066\u3088\u308a\u3044\u3063\u305d\u3046\u529b\u3092\u6301\u3064\u3088\u3046\u306b\u306a\u3063\u305f\u3002\u30ed\u30fc\u30de\u306e\u5c06\u8ecd\u30a2\u30a8\u30c6\u30a3\u30a6\u30b9\u306f\u3001\u305d\u308c\u4ee5\u524d\u306b\u30eb\u30fc\u30a2\u3068\u624b\u3092\u7d50\u3093\u3067\u304a\u308a\u3001\u307e\u305f\u30ac\u30ea\u30a2\u8ecd\u53f8\u4ee4\u5b98\u306e\u5730\u4f4d\u3092\u5b88\u308b\u305f\u3081\u306b\u30d5\u30f3\u65cf\u306e\u52a9\u3051\u3092\u501f\u308a\u305f\u3053\u3068\u3082\u3042\u3063\u305f\u3002\u30a2\u30a8\u30c6\u30a3\u30a6\u30b9\u306f\u3001\u30ed\u30fc\u30de\u306e\u5317\u90e8\u8af8\u5dde\u3092\u8352\u3089\u3057\u307e\u308f\u308b\u30d6\u30eb\u30b0\u30f3\u30c8\u65cf\u3092\u8a0e\u4f10\u3059\u308b\u9060\u5f81\u3067\u3001\u518d\u3073\u30d5\u30f3\u65cf\u3092\u50ad\u5175\u306b\u96c7\u3063\u305f\u3002\u30d5\u30f3\u65cf\u306f\u8c4a\u5bcc\u306a\u8ecd\u8cc7\u91d1\u306b\u3082\u306e\u3092\u8a00\u308f\u305b\u3001\u304b\u306d\u3066\u306e\u76ee\u6a19\u3067\u3042\u308b\u6771\u30ed\u30fc\u30de\u5e1d\u56fd\u306e\u653b\u7565\u3092\u518d\u958b\u3057\u3001435\u5e74\u306b\u306f\u5f31\u308a\u5207\u3063\u305f\u30d3\u30b6\u30f3\u30c1\u30f3\u306e\u9996\u8133\u3092\u606b\u559d\u3057\u3066\u30de\u30eb\u30b4\u30b9\u306e\u6761\u7d04\u306b\u8abf\u5370\u3055\u305b\u305f\u3002\u3053\u306e\u6761\u7d04\u306f\u6bce\u5e74\u30d3\u30b6\u30f3\u30c1\u30f3\u5074\u304c\u652f\u6255\u3063\u3066\u3044\u305f\u9ec4\u91d1\u767e\u6570\u5341\u30ad\u30ed\u3068\u3044\u3046\u8ca2\u7d0d\u91d1\u30922\u500d\u306b\u540a\u308a\u4e0a\u3052\u308b\u3082\u306e\u3060\u3063\u305f\u3068\u8a00\u308f\u308c\u3001\u30d5\u30f3\u65cf\u306e\u8cc7\u91d1\u529b\u3092\u3044\u3063\u305d\u3046\u5145\u5b9f\u3055\u305b\u308b\u3053\u3068\u3068\u306a\u3063\u305f\u3002\u3082\u3063\u3068\u3082\u3001\u4f8b\u306b\u3088\u3063\u3066\u30d3\u30b6\u30f3\u30c1\u30f3\u5074\u304c\u7fa9\u52d9\u306e\u5c65\u884c\u3092\u6020\u3063\u3066\u8ca2\u7d0d\u91d1\u3092\u652f\u6255\u308f\u306a\u304b\u3063\u305f\u305f\u3081\u3001\u30d5\u30f3\u65cf\u306f\u4ee5\u524d\u3068\u540c\u3058\u304f\u6771\u30ed\u30fc\u30de\u5e1d\u56fd\u306e\u9818\u5185\u306b\u4fb5\u5165\u3057\u3001\u3044\u304f\u3064\u3082\u306e\u90fd\u5e02\u3092\u8e42\u8e99\u3057\u305f\u3042\u3052\u304f\u3001443\u5e74\u306b\u306f\u3068\u3046\u3068\u3046\u30b3\u30f3\u30b9\u30bf\u30f3\u30c6\u30a3\u30ce\u30fc\u30d7\u30eb\u306b\u5230\u9054\u3057\u305f\u3002\u30d3\u30b6\u30f3\u30c1\u30f3\u5074\u306f\u518d\u5ea6\u9ad8\u984d\u306e\u8ca2\u7d0d\u91d1(\u4eca\u5ea6\u306f\u3082\u3068\u306e\u91d1\u984d\u306e3\u500d\u3068\u3055\u308c\u305f)\u3092\u652f\u6255\u3046\u3053\u3068\u306b\u540c\u610f\u3057\u3001\u30d5\u30f3\u65cf\u306f\u3053\u308c\u3092\u4fe1\u7528\u3057\u3066\u5e1d\u90fd\u304b\u3089\u5f15\u304d\u4e0a\u3052\u305f\u3002"
        },
        {
            "heading": "\u30a2\u30c3\u30c6\u30a3\u30e9\u306b\u3088\u308b\u7d71\u7387",
            "text": "445\u5e74\u3001\u5144\u30d6\u30ec\u30c0\u304c\u6b7b\u306b\u3001\u30a2\u30c3\u30c6\u30a3\u30e9\u306f\u30d5\u30f3\u5e1d\u56fd\u305f\u30601\u4eba\u306e\u7d71\u6cbb\u8005\u3068\u306a\u308b\u3002\u305d\u3057\u3066447\u5e74\u307e\u3067\u306b\u306f\u6771\u30ed\u30fc\u30de\u5e1d\u56fd\u306e\u8ecd\u52e2\u3092\u6253\u3061\u7834\u308a\u3001\u30ed\u30fc\u30de\u306e\u57ce\u7826\u3092\u6b21\u3005\u306b\u843d\u3068\u3057\u3066\u30d0\u30eb\u30ab\u30f3\u534a\u5cf6\u3092\u624b\u4e2d\u306b\u53ce\u3081\u3066\u3044\u305f\u3002\u30ed\u30fc\u30de\u4eba\u306f\u307e\u305f\u3057\u3066\u3082\u30d5\u30f3\u65cf\u306b\u8ca2\u7d0d\u91d1\u306e\u652f\u6255\u3044\u3092\u7533\u3057\u51fa\u3066\u3001\u5c11\u306a\u304f\u3068\u3082\u30a2\u30c3\u30c6\u30a3\u30e9\u306e\u95a2\u5fc3\u304c\u3088\u305d\u306b\u5411\u3051\u3089\u308c\u308b\u307e\u3067\u306e\u3057\u3070\u3089\u304f\u306e\u9593\u306f\u3001\u5b9f\u969b\u306b\u305d\u308c\u3092\u652f\u6255\u3044\u3082\u3057\u305f\u3002451\u5e74\u3001\u30a2\u30c3\u30c6\u30a3\u30e9\u306f\u30ac\u30ea\u30a2\u306b\u4fb5\u653b\u3057\u3001\u591a\u6570\u306e\u90fd\u5e02\u3092\u653b\u7565\u3057\u306a\u304c\u3089\u30aa\u30eb\u30ec\u30a2\u30f3\u306b\u9054\u3057\u305f\u3002\u76ae\u8089\u306b\u3082\u3001\u304b\u3064\u3066\u30d5\u30f3\u65cf\u3092\u652f\u63f4\u3057\u305f\u30ed\u30fc\u30de\u306e\u30a2\u30a8\u30c6\u30a3\u30a6\u30b9\u5c06\u8ecd\u304c\u3001\u30ac\u30ea\u30a2\u9632\u885b\u306e\u305f\u3081\u3001\u30d5\u30f3\u65cf\u3068\u771f\u3063\u5411\u304b\u3089\u306e\u5bfe\u6c7a\u3092\u5f37\u3044\u3089\u308c\u308b\u3053\u3068\u306b\u306a\u308b\u3002\u30a2\u30a8\u30c6\u30a3\u30a6\u30b9\u5c06\u8ecd\u306f\u30ed\u30fc\u30de\u3068\u897f\u30b4\u30fc\u30c8\u65cf\u306e\u9023\u5408\u8ecd\u3092\u7387\u3044\u3001\u30ab\u30bf\u30e9\u30a6\u30cc\u30e0\u306e\u5e73\u539f\u306b\u304a\u3044\u3066\u30a2\u30c3\u30c6\u30a3\u30e9\u3068\u6fc0\u7a81\u3001\u30d5\u30f3\u65cf\u306e\u9032\u6483\u3092\u963b\u6b62\u3059\u308b\u3053\u3068\u306b\u6210\u529f\u3057\u305f\u3002\u8ecd\u53f8\u4ee4\u5b98\u3068\u3057\u3066\u521d\u3081\u3066\u5927\u6557\u3092\u55ab\u3057\u305f\u30a2\u30c3\u30c6\u30a3\u30e9\u306f\u30ac\u30ea\u30a2\u304b\u3089\u64a4\u9000\u3057\u3001\u614b\u52e2\u306e\u7acb\u3066\u76f4\u3057\u3092\u4f59\u5100\u306a\u304f\u3055\u308c\u308b\u3002\u3057\u304b\u3057\u3001\u305d\u3053\u306f\u98fd\u304f\u306a\u304d\u5f81\u670d\u8005\u30a2\u30c3\u30c6\u30a3\u30e9\u306e\u3053\u3068\u3060\u3002\u4eca\u5ea6\u306f\u30a4\u30bf\u30ea\u30a2\u534a\u5cf6\u3092\u8972\u3046\u3068\u30ed\u30fc\u30de\u5e1d\u56fd\u306e\u4e2d\u67a2\u3092\u9665\u308c\u308b\u3079\u304f\u6771\u306b\u5175\u3092\u9032\u3081\u305f\u3002\u30a2\u30c3\u30c6\u30a3\u30e9\u306f\u5317\u30a4\u30bf\u30ea\u30a2\u306e\u8af8\u90fd\u5e02\u3092\u5236\u5727\u3057\u306a\u304c\u3089\u3082\u3001\u30ed\u30fc\u30de\u306b\u81f3\u308b\u3053\u3068\u306f\u3064\u3044\u306b\u306a\u304b\u3063\u305f\u3002\u30a2\u30c3\u30c6\u30a3\u30e9\u306b\u305d\u308c\u4ee5\u4e0a\u306e\u9032\u8ecd\u3092\u601d\u3044\u3068\u3069\u307e\u3089\u305b\u305f\u306e\u306f\u30ed\u30fc\u30de\u6cd5\u738b\u30ec\u30aa1\u4e16\u306e\u4ef2\u88c1\u3060\u3063\u305f\u3068\u3059\u308b\u6b74\u53f2\u5bb6\u304c\u3044\u308b\u4e00\u65b9\u3001\u6771\u30ed\u30fc\u30de\u5e1d\u56fd\u304b\u3089\u306e\u63f4\u8ecd\u3053\u305d\u304c\u6c7a\u610f\u3092\u63fa\u308b\u304c\u305b\u305f\u306e\u3060\u3068\u3044\u3046\u898b\u65b9\u3082\u3042\u308b\u3002\u3044\u305a\u308c\u306b\u305b\u3088\u3001\u30a2\u30c3\u30c6\u30a3\u30e9\u306f\u305d\u308c\u304b\u3089\u307b\u3069\u306a\u304f\u3057\u3066\u3001\u81ea\u8eab\u306e\u5a5a\u793c\u304c\u55b6\u307e\u308c\u305f\u6669\u306b\u6025\u6b7b\u3057\u3066\u3057\u307e\u3046\u3002\u4e00\u90e8\u306e\u6b74\u53f2\u5bb6\u306b\u3088\u308c\u3070\u3001\u305f\u3093\u306b\u9f3b\u8840\u3092\u6d41\u3057\u305f\u3060\u3051\u3067\u5012\u308c\u3001\u305d\u306e\u307e\u307e\u606f\u7d76\u3048\u305f\u3068\u3044\u3046\u3002\u3068\u304d\u306b453\u5e74\u3002\u30ab\u30ea\u30b9\u30de\u7684\u6307\u5c0e\u8005\u30a2\u30c3\u30c6\u30a3\u30e9\u306e\u5f37\u529b\u306a\u30ea\u30fc\u30c0\u30fc\u30b7\u30c3\u30d7\u3068\u305d\u306e\u5a01\u5149\u3092\u5931\u3063\u305f\u3053\u3068\u306b\u3088\u308a\u300c\u8ab0\u306b\u3082\u6b62\u3081\u3089\u308c\u306a\u3044\u8ecd\u52e2\u300d\u3068\u3044\u3046\u30d5\u30f3\u65cf\u306e\u30a4\u30e1\u30fc\u30b8\u306f\u5265\u304c\u308c\u843d\u3061\u308b\u3002"
        },
        {
            "heading": "\u30a2\u30c3\u30c6\u30a3\u30e9\u4ee5\u5f8c",
            "text": "\u30a2\u30c3\u30c6\u30a3\u30e9\u306e\u6b7b\u5f8c\u3001\u30d5\u30f3\u65cf\u306e\u5e1d\u56fd\u306f\u5f7c\u306e\u9577\u5b50\u30a8\u30e9\u30af\u304c\u6cbb\u3081\u308b\u306f\u305a\u3060\u3063\u305f\u3002\u3057\u304b\u3057\u5f1f\u306e\u30c7\u30f3\u30ad\u30b8\u30c3\u30af\u3068\u30a4\u30eb\u30ca\u30c3\u30af\u304c\u5f8c\u7d99\u8005\u4e89\u3044\u306b\u540d\u4e57\u308a\u3092\u4e0a\u3052\u305f\u3053\u3068\u3067\u3001\u5e1d\u56fd\u306f\u5185\u90e8\u5206\u88c2\u306b\u9665\u308a\u3001\u6025\u901f\u306b\u74e6\u89e3\u3057\u3066\u3044\u304f\u3002\u7d71\u6cbb\u958b\u59cb\u304b\u3089\u308f\u305a\u304b1\u5e74\u5f8c\u306e454\u5e74\u3001\u30a8\u30e9\u30af\u306f\u30cd\u30c0\u30aa\u5ddd\u306e\u6226\u3044\u3067\u6226\u6b7b\u3002\u3053\u306e\u6226\u3044\u306b\u304a\u3044\u3066\u3001\u30d5\u30f3\u65cf\u306f\u30b2\u30eb\u30de\u30f3\u8af8\u90e8\u65cf\u306e\u9023\u5408\u8ecd\u306b\u5b8c\u6557\u3057\u305f\u3002\u5144\u306e\u6b7b\u5f8c\u3001\u30c7\u30f3\u30ad\u30b8\u30c3\u30af\u304c\u3064\u304b\u306e\u9593\u738b\u4f4d\u3092\u7d99\u3044\u3060\u3082\u306e\u306e\u3001\u5e1d\u56fd\u306f\u3059\u3067\u306b\u5d29\u58ca\u306e\u4e00\u9014\u3092\u305f\u3069\u3063\u3066\u3044\u305f\u3002\u30d5\u30f3\u65cf\u306f\u767b\u5834\u3057\u305f\u6642\u3068\u540c\u3058\u304f\u3089\u3044\u5510\u7a81\u306b\u6b74\u53f2\u306e\u8868\u821e\u53f0\u304b\u3089\u59ff\u3092\u6d88\u3059\u3002469\u5e74\u306b\u30c7\u30f3\u30ad\u30b8\u30c3\u30af\u304c\u6b7b\u306c\u3068\u3001\u30d5\u30f3\u65cf\u306e\u6b8b\u515a\u306f\u3001\u5f7c\u3089\u306e\u6545\u90f7\u3067\u3042\u308b\u4e2d\u592e\u30a2\u30b8\u30a2\u306e\u8349\u539f\u5730\u5e2f\u306b\u5e30\u3063\u3066\u3044\u3063\u305f\u3068\u8003\u3048\u3089\u308c\u3066\u3044\u308b\u3002"
        },
        {
            "heading": "\u30d5\u30f3\u65cf\u306e\u793e\u4f1a",
            "text": "\u30d5\u30f3\u65cf\u306f\u305d\u306e\u8d77\u6e90\u3053\u305d\u8b0e\u306b\u5305\u307e\u308c\u3066\u3044\u308b\u304c\u3001\u99ac\u306b\u307e\u305f\u304c\u3063\u305f\u307e\u307e\u98df\u4e8b\u3092\u3057\u3001\u7720\u308a\u3001\u6b7b\u3093\u3060\u3068\u8a00\u308f\u308c\u3066\u3044\u308b\u3002\u5f7c\u3089\u304c\u5353\u8d8a\u3057\u305f\u9a0e\u99ac\u6226\u8853\u3068\u4e57\u99ac\u8853\u3067\u6b74\u53f2\u306b\u540d\u3092\u6b8b\u3057\u305f\u306e\u3082\u3046\u306a\u305a\u3051\u308b\u3068\u3044\u3046\u3082\u306e\u3060\u3002\u30d5\u30f3\u65cf\u306f\u4e3b\u306b\u7267\u755c\u3067\u66ae\u3089\u3057\u3092\u7acb\u3066\u3066\u3044\u305f\u3053\u3068\u304b\u3089\u3001\u3055\u307e\u3056\u307e\u306a\u5bb6\u755c\u3092\u6240\u6709\u3057\u3066\u3044\u305f\u3002\u305d\u306e\u305f\u3081\u3001\u65c5\u304b\u3089\u65c5\u3078\u306e\u66ae\u3089\u3057\u3092\u9001\u3063\u3066\u3082\u98df\u6599\u306b\u56f0\u308b\u3053\u3068\u306f\u306a\u304f\u3001\u307e\u305f\u8863\u985e\u306b\u52a0\u5de5\u3059\u308b\u305f\u3081\u306e\u76ae\u306b\u4e8b\u6b20\u304f\u5fc3\u914d\u3082\u306a\u304b\u3063\u305f\u3002\u30d5\u30f3\u65cf\u306f\u751f\u307e\u308c\u306a\u304c\u3089\u306e\u5f13\u5175\u3067\u3082\u3042\u308a\u3001\u983b\u7e41\u306a\u72e9\u308a\u3092\u901a\u3058\u3066\u8155\u3092\u78e8\u304d\u3001\u7279\u5225\u306b\u3042\u3064\u3089\u3048\u305f\u8907\u5408\u5f13\u3092\u4f7f\u3063\u305f\u3002\u5f7c\u3089\u304c\u7372\u7269\u306b\u3068\u3063\u3066\u3082\u6575\u306b\u3068\u3063\u3066\u3082\u6050\u308b\u3079\u304d\u76f8\u624b\u3060\u3063\u305f\u3053\u3068\u306f\u9593\u9055\u3044\u306a\u3044\u3002\u5f81\u670d\u8005\u3068\u3057\u3066\u306e\u9854\u306b\u3070\u304b\u308a\u76ee\u304c\u884c\u304f\u304c\u3001\u30d5\u30f3\u65cf\u306f\u4ef2\u9593\u5185\u306b\u8155\u306e\u826f\u3044\u7d30\u5de5\u5e2b\u3084\u8077\u4eba\u3092\u62b1\u3048\u3066\u3082\u3044\u305f\u3002\u30d5\u30f3\u65cf\u306e\u4f4f\u3093\u3067\u3044\u305f\u5730\u57df\u304b\u3089\u306f\u3001\u5f7c\u3089\u306e\u624b\u306b\u3088\u308b\u3082\u306e\u3068\u601d\u308f\u308c\u308b\u9752\u9285\u306e\u907a\u7269\u304c\u591a\u6570\u898b\u3064\u304b\u3063\u3066\u3044\u308b\u3002\u30d5\u30f3\u65cf\u306e\u846c\u5100\u306b\u4f7f\u308f\u308c\u305f\u3068\u8003\u3048\u3089\u308c\u308b\u9752\u9285\u306e\u5927\u91dc\u3082\u305d\u306e1\u3064\u3060\u3002"
        },
        {
            "heading": "\u30d5\u30f3\u65cf\u306e\u793e\u4f1a",
            "text": "\u30d5\u30f3\u65cf\u306f\u305d\u306e\u8d77\u6e90\u3053\u305d\u8b0e\u306b\u5305\u307e\u308c\u3066\u3044\u308b\u304c\u3001\u99ac\u306b\u307e\u305f\u304c\u3063\u305f\u307e\u307e\u98df\u4e8b\u3092\u3057\u3001\u7720\u308a\u3001\u6b7b\u3093\u3060\u3068\u8a00\u308f\u308c\u3066\u3044\u308b\u3002\u5f7c\u3089\u304c\u5353\u8d8a\u3057\u305f\u9a0e\u99ac\u6226\u8853\u3068\u4e57\u99ac\u8853\u3067\u6b74\u53f2\u306b\u540d\u3092\u6b8b\u3057\u305f\u306e\u3082\u3046\u306a\u305a\u3051\u308b\u3068\u3044\u3046\u3082\u306e\u3060\u3002\u30d5\u30f3\u65cf\u306f\u4e3b\u306b\u7267\u755c\u3067\u66ae\u3089\u3057\u3092\u7acb\u3066\u3066\u3044\u305f\u3053\u3068\u304b\u3089\u3001\u3055\u307e\u3056\u307e\u306a\u5bb6\u755c\u3092\u6240\u6709\u3057\u3066\u3044\u305f\u3002\u305d\u306e\u305f\u3081\u3001\u65c5\u304b\u3089\u65c5\u3078\u306e\u66ae\u3089\u3057\u3092\u9001\u3063\u3066\u3082\u98df\u6599\u306b\u56f0\u308b\u3053\u3068\u306f\u306a\u304f\u3001\u307e\u305f\u8863\u985e\u306b\u52a0\u5de5\u3059\u308b\u305f\u3081\u306e\u76ae\u306b\u4e8b\u6b20\u304f\u5fc3\u914d\u3082\u306a\u304b\u3063\u305f\u3002\u30d5\u30f3\u65cf\u306f\u751f\u307e\u308c\u306a\u304c\u3089\u306e\u5f13\u5175\u3067\u3082\u3042\u308a\u3001\u983b\u7e41\u306a\u72e9\u308a\u3092\u901a\u3058\u3066\u8155\u3092\u78e8\u304d\u3001\u7279\u5225\u306b\u3042\u3064\u3089\u3048\u305f\u8907\u5408\u5f13\u3092\u4f7f\u3063\u305f\u3002\u5f7c\u3089\u304c\u7372\u7269\u306b\u3068\u3063\u3066\u3082\u6575\u306b\u3068\u3063\u3066\u3082\u6050\u308b\u3079\u304d\u76f8\u624b\u3060\u3063\u305f\u3053\u3068\u306f\u9593\u9055\u3044\u306a\u3044\u3002\u5f81\u670d\u8005\u3068\u3057\u3066\u306e\u9854\u306b\u3070\u304b\u308a\u76ee\u304c\u884c\u304f\u304c\u3001\u30d5\u30f3\u65cf\u306f\u4ef2\u9593\u5185\u306b\u8155\u306e\u826f\u3044\u7d30\u5de5\u5e2b\u3084\u8077\u4eba\u3092\u62b1\u3048\u3066\u3082\u3044\u305f\u3002\u30d5\u30f3\u65cf\u306e\u4f4f\u3093\u3067\u3044\u305f\u5730\u57df\u304b\u3089\u306f\u3001\u5f7c\u3089\u306e\u624b\u306b\u3088\u308b\u3082\u306e\u3068\u601d\u308f\u308c\u308b\u9752\u9285\u306e\u907a\u7269\u304c\u591a\u6570\u898b\u3064\u304b\u3063\u3066\u3044\u308b\u3002\u30d5\u30f3\u65cf\u306e\u846c\u5100\u306b\u4f7f\u308f\u308c\u305f\u3068\u8003\u3048\u3089\u308c\u308b\u9752\u9285\u306e\u5927\u91dc\u3082\u305d\u306e1\u3064\u3060\u3002"
        }
    ],
    "leader": {
        "name": "\u30a2\u30c3\u30c6\u30a3\u30e9",
        "subtitle": "\u30d5\u30f3\u5e1d\u56fd\u306e\u738b",
        "lived": "\u7d00\u5143406\u5e74\u9803 - 453\u5e74",
