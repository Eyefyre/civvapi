    "dawn_of_man": "\u9858\u4e3b\u8cdc\u798f\u65bc\u60a8\uff0c\u6700\u89aa\u5207\u7684\u746a\u9e97\u4e9e\u5973\u738b\uff0c\u8461\u8404\u7259\u7684\u4fdd\u8b77\u8005\u8207\u4eba\u6c11\u7684\u541b\u4e3b\u3002\u60a8\u662f\u8461\u8404\u7259\u53f2\u4e0a\u7b2c\u4e00\u4f4d\u5973\u738b\uff0c\u660e\u667a\u7684\u9818\u5c0e\u4fdd\u8b77\u4e86\u738b\u5ba4\u514d\u906d\u62ff\u7834\u5d19\u7684\u5165\u4fb5\uff0c\u4e26\u5728\u5df4\u897f\u5efa\u7acb\u4e00\u500b\u5b89\u5168\u7684\u907f\u98a8\u6e2f\uff0c\u7dad\u8b77\u8461\u8404\u7259\u7684\u5b8c\u6574\u653f\u6b0a\u3002\u5118\u7ba1\u5728\u60a8\u7d71\u6cbb\u671f\u9593\u4e0d\u65b7\u8207\u75c5\u9b54\u6297\u6230\uff0c\u60a8\u9084\u662f\u81f4\u529b\u65bc\u8207\u5df4\u897f\u7684\u5bc6\u5207\u5408\u4f5c\uff0c\u5c0d\u65bc\u8461\u8404\u7259\u6700\u7d42\u7372\u5f97\u7368\u7acb\u6709\u5f88\u5927\u7684\u5e6b\u52a9\uff0c\u800c\u60a8\u56e0\u6b64\u5c07\u6c38\u9060\u53d7\u5230\u5169\u570b\u4eba\u6c11\u7684\u6b3d\u4f69\u8207\u5c0a\u91cd\u3002\u54e6\uff0c\u9ad8\u8cb4\u8654\u8aa0\u7684\u5973\u738b\uff0c\u60a8\u6703\u518d\u6b21\u4ee5\u5408\u4f5c\u8207\u8208\u76db\u7684\u7cbe\u795e\u9818\u5c0e\u8461\u8404\u7259\u738b\u570b\u55ce\uff1f\u60a8\u6703\u5efa\u7acb\u4e00\u500b\u8d85\u8d8a\u524d\u4eba\u7684\u65b0\u6b77\u53f2\u907a\u8de1\u55ce\uff1f\u60a8\u80fd\u5efa\u7acb\u4e00\u500b\u7d93\u5f97\u8d77\u6642\u9593\u8003\u9a57\u7684\u6587\u660e\u55ce\uff1f",
