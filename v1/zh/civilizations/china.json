    "dawn_of_man": "\u4e0a\u84bc\u7684\u795d\u798f\u8207\u60a8\u540c\u5728\uff0c\u6b66\u5247\u5929\u5973\u7687\uff0c\u4e2d\u570b\u6700\u7f8e\u9e97\u3001\u6700\u50b2\u6162\u7684\u7d71\u6cbb\u8005\uff01\u5049\u5927\u7684\u5973\u7687\uff0c\u60a8\u7684\u9b45\u5f71\u4f7f\u9bae\u82b1\u7dbb\u653e\u3001\u8b93\u6cb3\u6c34\u6d41\u52d5\uff01\u60a8\u662f\u4e2d\u570b\u7684\u9818\u5c0e\u8005\uff0c\u9019\u500b\u4eba\u985e\u6b77\u53f2\u4e2d\u6700\u60a0\u4e45\u3001\u6700\u5049\u5927\u7684\u6587\u660e\u3002\u4e2d\u570b\u7684\u6b77\u53f2\u80fd\u8ffd\u6714\u5230\u6df7\u6c8c\u7684\u4e0a\u53e4\uff0c\u4e2d\u570b\u4eba\u5728\u5176\u4ed6\u6587\u660e\u8a95\u751f\u4e4b\u524d\u5c31\u5df2\u7d93\u6709\u8a31\u591a\u7684\u6210\u5c31\u3002\u4e2d\u570b\u5c0d\u85dd\u8853\u53ca\u79d1\u5b78\u7684\u8ca2\u737b\u5be6\u5728\u662f\u592a\u591a\u3001\u592a\u5947\u5999\uff0c\u4e0d\u662f\u4e09\u8a00\u5169\u8a9e\u5c31\u80fd\u9053\u76e1\u3002\u5370\u5237\u8853\u3001\u706b\u85e5\u3001\u5b54\u5b50\u7684\u8457\u4f5c\uff0c\u9019\u4e9b\u90fd\u53ea\u662f\u4e2d\u570b\u7d66\u4e88\u9019\u4e16\u754c\u4e0d\u914d\u64c1\u6709\u7684\u4e00\u5c0f\u90e8\u5206\u79ae\u7269\uff01\u5049\u5927\u7684\u5973\u7687\uff0c\u60a8\u7cbe\u660e\u53c8\u7f8e\u9e97\uff0c\u5f9e\u5351\u5fae\u7684\u59be\u5ba4\u5230\u6210\u70ba\u4e00\u4f4d\u795e\u8056\u7684\u5973\u7687\u2014\u60a8\u7684\u4eba\u6c11\u6b63\u547c\u559a\u8457\u60a8\u4f86\u7d71\u9818\u4ed6\u5011\uff01\u5049\u5927\u7684\u4e2d\u570b\u518d\u4e00\u6b21\u53d7\u5230\u5404\u65b9\u883b\u5937\u7684\u4fb5\u64fe\u3002\u60a8\u80fd\u5920\u6253\u6557\u9019\u4e9b\u6575\u4eba\u4e26\u518d\u4e00\u6b21\u58ef\u5927\u60a8\u7684\u570b\u5bb6\u55ce\uff1f\u60a8\u80fd\u5efa\u8d77\u4e00\u500b\u7d93\u5f97\u4f4f\u6642\u9593\u8003\u9a57\u7684\u6587\u660e\u55ce\uff1f",
