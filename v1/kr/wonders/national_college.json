    "game_info": "\ubaa8\ub4e0 \ub3c4\uc2dc\uc5d0 \ub3c4\uc11c\uad00\uc774 \uc788\uc5b4\uc57c \ud569\ub2c8\ub2e4. \ub3c4\uc2dc\uac00 \ub9ce\uc744\uc218\ub85d \ube44\uc6a9\uc774 \ub298\uc5b4\ub0a9\ub2c8\ub2e4.",
    "strategy": "\uad6d\ub9bd\ub300\ud559 \uad6d\uac00 \ubd88\uac00\uc0ac\uc758\uac00 \uc788\uc73c\uba74 \ub3c4\uc2dc\uc758 \uacfc\ud559 \uc0dd\uc131\ub7c9\uc774 50%, \ubb38\ud654\uac00 1\ub9cc\ud07c \uc99d\uac00\ud569\ub2c8\ub2e4. \uad6d\ub9bd\ub300\ud559\uc744 \uac74\uc124\ud558\ub824\uba74 \ubaa8\ub4e0 \ub3c4\uc2dc\uc5d0 \ub3c4\uc11c\uad00\uc774 \uc788\uc5b4\uc57c \ud569\ub2c8\ub2e4.",
    "historical_info": "\uad6d\ub9bd\ub300\ud559\uc740 \uc815\ubd80\uac00 \uc6b4\uc601\ud558\ub294 \uace0\ub4f1 \uad50\uc721 \uae30\uad00\uc774\ub2e4. \uadfc\ub300 \ub300\ud559\uc758 \uae30\uc6d0\uc740 \uc911\uc138\uc2dc\ub300\uc5d0 \uc774\uc2ac\ub78c\uad50 \uc0ac\uc6d0\uacfc \uc5f0\uacc4\ud574 \ubc95\uacfc \uc2e0\ud559\uc744 \uac00\ub974\uce58\ub358 \uae30\uad00\uc778 \ub9c8\ub4dc\ub77c\uc0ac\uae4c\uc9c0 \uac70\uc2ac\ub7ec \uc62c\ub77c\uac04\ub2e4. \uc624\ub298\ub0a0\uc5d0\ub294 \uc138\uacc4 \uac70\uc758 \ubaa8\ub4e0 \uad6d\uac00\uac00 \uad6d\ub9bd \ub300\ud559 \ubc0f \ub300\ud559 \uc81c\ub3c4\ub97c \uac16\ucd94\uace0 \uc788\ub2e4. \ub7ec\uc2dc\uc544 \uc138\uacc4 \ub300\ud559 \uc21c\uc704\uc5d0 \ub530\ub974\uba74 \uc544\uc2dc\uc544 \ucd5c\uace0, \uc138\uacc4\uc5d0\uc11c 3\ub4f1\uc744 \uc790\ub791\ud558\ub294 \uc77c\ubcf8\uc758 \ub3d9\uacbd \ub300\ud559\uc774 \uadf8\ub7ec\ud55c \uacf5\uacf5 \uad50\uc721 \uae30\uad00\uc774\ub2e4.",
