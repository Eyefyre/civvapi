    "game_info": "\ub3c4\uc2dc\uc5d0\uc11c \uc6d0\uc790\ud3ed\ud0c4\uacfc \ud575\ubbf8\uc0ac\uc77c\uc744 \ub9cc\ub4e4 \uc218 \uc788\uc2b5\ub2c8\ub2e4.",
    "strategy": "\ub9e8\ud574\ud2bc \ud504\ub85c\uc81d\ud2b8\ub294 \ud575 \ubb34\uae30 \uc81c\uc791\uc744 \uac00\ub2a5\ud558\uac8c \ud569\ub2c8\ub2e4. \ud575\ubbf8\uc0ac\uc77c\uc744 \ub9cc\ub4e4\ub824\ub294 \ubaa8\ub4e0 \ubb38\uba85\uc740 \ub9e8\ud574\ud2bc \ud504\ub85c\uc81d\ud2b8\ub97c \uc644\ub8cc\ud574\uc57c \ud569\ub2c8\ub2e4.",
    "historical_info": "\ub9e8\ud574\ud2bc \ud504\ub85c\uc81d\ud2b8\ub294 \uc81c2\ucc28 \uc138\uacc4\ub300\uc804 \uc911 \ud575\ud3ed\ud0c4\uc744 \uac1c\ubc1c\ud558\ub824\ub294 \uc601\uad6d\uacfc \uc720\ub7fd\uc758 \uc5f0\uad6c\ub97c \ubd80\ub974\ub294 \uc554\ud638\uc600\ub2e4. \uacc4\ud68d\uc740 \ubbf8\uad6d\uc774 \uc774\ub04c\uc5c8\uc9c0\ub9cc \uc601\uad6d\uacfc \uce90\ub098\ub2e4\uc5d0\uc11c \uc628 \uacfc\ud559\uc790\ub4e4\uacfc \uae30\uc220\uc790\ub4e4\uc774 \ucc38\uc5ec\ud588\ub2e4. \ubbf8\uad6d\uc740 1930\ub144\ub300\ubd80\ud130 \ud575\ud3ed\ud0c4 \uac1c\ubc1c\uc5d0 \uc0c1\ub2f9\ud55c \ub178\ub825\uc744 \uae30\uc6b8\uc600\uc9c0\ub9cc 1941\ub144 \uc77c\ubcf8\uc774 \uc9c4\uc8fc\ub9cc\uc744 \uc2b5\uaca9\ud558\uc790 \ucd5c\uace0\uc870\uc5d0 \uc774\ub974\ub800\ub2e4. \uc57d 125,000\uba85\uc758 \uc778\ub825\uacfc 200\uc870 \uc6d0\uc758 \uc790\uae08\uc744 \ud22c\uc790\ud55c \ub05d\uc5d0 1945\ub144 \ud575\ubb34\uae30\ub97c \uac1c\ubc1c\ud558\uace0 \uc2e4\ud5d8\ud558\ub294 \ub370 \uc131\uacf5\ud588\ub2e4. \ubbf8\uad6d\uc740 1945\ub144 8\uc6d4 \ucd08\uc5d0 \uc6b0\ub77c\ub284\uacfc \ud50c\ub8e8\ud1a0\ub284 \ud3ed\ud0c4\uc744 \uc77c\ubcf8\uc5d0 \ud22c\ud558\ud588\uace0 \uadf8\ub85c\ubd80\ud130 \uc77c\uc8fc\uc77c\uc774 \uc9c0\ub098\uae30 \uc804\uc5d0 \uc77c\ubcf8\uc758 \ud56d\ubcf5\uc744 \ubc1b\uc544\ub0b4 \uc81c2\ucc28 \uc138\uacc4\ub300\uc804\uc744 \uc885\uacb0\ud588\ub2e4.",
