    "dawn_of_man": "\u9858\u795e\u7684\u8cdc\u798f\u8207\u60a8\uff0c\u6df1\u53d7\u611b\u6234\u7684\u6ce2\u65af\u738b\u5927\u6d41\u58eb\u3002\u60a8\u9818\u5c0e\u8457\u4e00\u7fa4\u5805\u5f37\u800c\u8070\u6167\u7684\u4eba\u6c11\u3002\u5728\u4e16\u754c\u6587\u660e\u7684\u4f0a\u59cb\uff0c\u5049\u5927\u7684\u6ce2\u65af\u738b\u5c45\u9b6f\u58eb\u53cd\u6297\u8d77\u5f37\u5927\u7684\u7c73\u5e95\u5b89\u5e1d\u570b\uff0c\u5728\u897f\u5143\u524d550\u5e74\uff0c\u4fbf\u5c07\u7c73\u5e95\u5b89\u4eba\u5fb9\u5e95\u62b9\u53bb\u3002\u5c45\u9b6f\u58eb\u5927\u5e1d\u4ee5\u9748\u6d3b\u7684\u5916\u4ea4\u8207\u52c7\u731b\u7684\u8ecd\u968a\u5f81\u670d\u4e86\u5bcc\u5eb6\u7684\u5442\u5e95\u4e9e\u738b\u570b\u548c\u5f37\u5927\u7684\u5df4\u6bd4\u502b\u738b\u570b\uff0c\u800c\u9a55\u50b2\u7684\u57c3\u53ca\u4e5f\u5728\u6578\u5e74\u4e4b\u5f8c\u88ab\u5176\u5b50\u5f81\u670d\u3002\u96a8\u8457\u6642\u9593\u7684\u63a8\u79fb\uff0c\u6ce2\u65af\u7684\u7248\u5716\u4e00\u76f4\u64f4\u5c55\u5230\u4e86\u99ac\u5176\u9813\uff0c\u903c\u8fd1\u5230\u4e86\u5d1b\u8d77\u7684\u5e0c\u81d8\u57ce\u90a6\u7684\u5927\u9580\u3002\u6ce2\u65af\u738b\u671d\u672c\u53ef\u6e90\u9060\u6d41\u9577\uff0c\u4f46\u5728\u4e00\u6b21\u53ef\u6015\u7684\u6c7a\u6230\u4e2d\uff0c\u99ac\u5176\u9813\u60e1\u5f92\u4e9e\u6b77\u5c71\u5927\u6467\u6bc0\u4e86\u9019\u500b\u5049\u5927\u7684\u5e1d\u570b\u3002\u5927\u6d41\u58eb\u554a\uff0c\u60a8\u7684\u4eba\u6c11\u6b63\u5f15\u9838\u76fc\u671b\u60a8\u80fd\u518d\u6b21\u91cd\u73fe\u6ce2\u65af\u7684\u6b0a\u529b\u8207\u69ae\u8000\uff01\u60a8\u5148\u7956\u5011\u7684\u5e1d\u570b\u4e00\u5b9a\u6703\u91cd\u65b0\u5d1b\u8d77\uff0c\u78be\u788e\u6575\u4eba\u4e26\u8b93\u548c\u5e73\u8207\u79e9\u5e8f\u91cd\u73fe\u4e16\u9593\u3002\u5049\u5927\u7684\u738b\u554a\uff0c\u60a8\u6703\u56de\u61c9\u4ed6\u5011\u7684\u547c\u6c42\u55ce\uff1f\u60a8\u80fd\u5efa\u8d77\u4e00\u500b\u7d93\u5f97\u4f4f\u6642\u9593\u8003\u9a57\u7684\u6587\u660e\u55ce\uff1f",
