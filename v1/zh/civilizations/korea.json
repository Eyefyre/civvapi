    "dawn_of_man": "\u60a8\u597d\uff0c\u9ad8\u8cb4\u7684\u671d\u9bae\u4e16\u5b97\u5927\u738b\u674e\u7979\uff0c\u4eba\u6c11\u7684\u516c\u50d5\u8207\u671d\u9bae\u738b\u671d\u7684\u4fdd\u8b77\u8005\u3002\u60a8\u5c0d\u660c\u69ae\u7e41\u76db\u7684\u9060\u8996\u53ca\u5c0d\u5e73\u6c11\u7684\u5ee3\u65bd\u6069\u6fa4\uff0c\u4f7f\u60a8\u6210\u70ba\u6240\u6709\u671d\u9bae\u541b\u4e3b\u4e2d\u6700\u53d7\u4eba\u6c11\u611b\u6234\u7684\u4e00\u4f4d\u3002\u5f9e\u7d71\u6cbb\u4f0a\u59cb\uff0c\u60a8\u5c31\u70ba\u63d0\u4f9b\u516c\u5e73\u8207\u5408\u7406\u7684\u793e\u6703\u52aa\u529b\uff0c\u50c5\u6709\u60a8\u5c0d\u65bc\u667a\u6167\u6c38\u4e0d\u6b62\u606f\u7684\u5c0b\u6c42\u9a45\u7b56\u8457\u79d1\u6280\u7684\u9032\u6b65\u3002\u7389\u6bbf\u7684\u5b78\u8005\u5011\u5728\u60a8\u7684\u667a\u6167\u5f15\u5c0e\u4e0b\uff0c\u5275\u9020\u51fa\u671d\u9bae\u7684\u66f8\u5beb\u6587\u5b57\u2014\u8afa\u6587\uff0c\u70ba\u6578\u4e16\u7d00\u7684\u6587\u5b78\u9ed1\u6697\u5e36\u4f86\u4e86\u5149\u660e\uff0c\u4e5f\u5e36\u52d5\u4e86\u5927\u898f\u6a21\u7684\u79d1\u5b78\u5275\u65b0\u3002\u53ef\u656c\u7684\u4e16\u5b97\u554a\uff0c\u4eba\u6c11\u518d\u4e00\u6b21\u9700\u8981\u60a8\u4f86\u5e36\u9818\u4ed6\u5011\uff01\u60a8\u80fd\u5426\u56e0\u6b64\u800c\u8208\u8d77\uff0c\u70ba\u4eba\u6c11\u5e36\u4f86\u548c\u8ae7\u8207\u9ad4\u9762\u7684\u751f\u6d3b\uff1f\u60a8\u80fd\u4e0d\u80fd\u518d\u4e00\u6b21\u5c07\u60a8\u7684\u738b\u570b\u9818\u81f3\u4ee4\u4eba\u4e0d\u53ef\u601d\u8b70\u7684\u5883\u754c\uff1f\u60a8\u80fd\u5efa\u8d77\u4e00\u500b\u7d93\u5f97\u4f4f\u6642\u9593\u8003\u9a57\u7684\u6587\u660e\u55ce\uff1f",
