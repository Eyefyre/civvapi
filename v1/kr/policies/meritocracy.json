    "tenet_level": 0,
    "branch": {
        "name": "\uc790\uc720",
        "game_info": "\uc790\uc720\ub294 \ube60\ub978 \ud655\uc7a5\uc744 \uc6d0\ud558\ub294 \ubb38\uba85\uc5d0 \uc801\ud569\ud569\ub2c8\ub2e4.\uc790\uc720\ub97c \ucc44\ud0dd\ud558\uba74 \ubaa8\ub4e0 \ub3c4\uc2dc\uc5d0\uc11c \ubb38\ud654\ub97c 1\ub9cc\ud07c \uc0dd\uc131\ud569\ub2c8\ub2e4. \ud53c\ub77c\ubbf8\ub4dc\ub97c \uac74\uc124\ud560 \uc218 \uc788\uc2b5\ub2c8\ub2e4.\uc790\uc720 \uacc4\uc5f4\uc758 \ubaa8\ub4e0 \uc815\ucc45\uc744 \ucc44\ud0dd\ud558\uba74 \uc6d0\ud558\ub294 \uc704\uc778\uc774 \uc218\ub3c4 \uadfc\ucc98\uc5d0 \ucd9c\ud604\ud569\ub2c8\ub2e4.",
        "prereq_era": null,
        "historical_info": "\ud558\ub098\uc758 \uc815\ucc45\uc73c\ub85c\uc11c \uc790\uc720\ub294 \ud55c \uad6d\uac00\uc758 \uc2dc\ubbfc\uc740 \uc790\uc720\ub85c\uc6b8 \uad8c\ub9ac\uac00 \uc788\ub2e4\ub294 \uae30\ubcf8\uc801\uc778 \uac00\uc815\uc73c\ub85c \uc815\uc758\ud560 \uc218 \uc788\ub2e4. \uc2e0\uccb4\uc801\uc778 \uc5b5\uc555\uacfc \uc804\uc81c\uc801\uc778 \ud1b5\uc81c\ub85c\ubd80\ud130 \uc790\uc720\ub85c\uc6cc\uc57c \ud55c\ub2e4. \ub18d\ub178\uc640 \ub178\uc608 \uadf8\ub9ac\uace0 \uc138\uc2b5\ub418\uc5b4 \ub0b4\ub824\uc628 \ud558\uc778\uc740 \uc790\uc720\ub97c \ub204\ub9b4 \uc218 \uc5c6\uc73c\uba70 \ud2b9\uc815\ud55c \ub9c8\uc744\uc774\ub098 \uc218\uc6a9\uc18c \ub610\ub294 \ube48\ubbfc\uac00\uc5d0 \uc0b4\ub3c4\ub85d \uac15\uc694\ub2f9\ud55c \uc2dc\ubbfc \uc5ed\uc2dc \uc790\uc720\ub86d\uc9c0 \ubabb\ud558\ub2e4."
    }
